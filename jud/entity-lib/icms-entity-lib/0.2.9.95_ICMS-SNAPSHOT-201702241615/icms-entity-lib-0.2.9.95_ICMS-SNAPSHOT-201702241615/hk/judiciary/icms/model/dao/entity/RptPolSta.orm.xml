        <basic name="rptPolStaCd">
            <column name="rpt_pol_sta_cd" />
        </basic>

        <basic name="rptPolStaDesc">
            <column name="rpt_pol_sta_desc" />
        </basic>

        <basic name="rptPolStaDescChin">
