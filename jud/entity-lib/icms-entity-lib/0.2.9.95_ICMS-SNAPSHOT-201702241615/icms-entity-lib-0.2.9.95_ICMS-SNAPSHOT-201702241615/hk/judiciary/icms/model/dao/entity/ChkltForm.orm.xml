<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.ChkltForm" access="FIELD">
     <table name="tchklt_form" />

     <attributes>
        <id name="chkltFormId">
            <column name="chklt_form_id" />
            <generated-value strategy="TABLE" generator="TCHKLT_FORM_SEQ" />
            <table-generator name="TCHKLT_FORM_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tchklt_form_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="chkltFormGenDt">
            <column name="chklt_form_gen_dt" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="status">
            <column name="status" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="hrnDtl" fetch="LAZY">
        <join-column name="hrn_dtl_id" referenced-column-name="hrn_dtl_id" />
    </many-to-one>

    <many-to-one name="doc" fetch="LAZY">
        <join-column name="doc_id" referenced-column-name="doc_id" />
    </many-to-one>

    <many-to-one name="tmpl" fetch="LAZY">
        <join-column name="tmpl_id" referenced-column-name="tmpl_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
