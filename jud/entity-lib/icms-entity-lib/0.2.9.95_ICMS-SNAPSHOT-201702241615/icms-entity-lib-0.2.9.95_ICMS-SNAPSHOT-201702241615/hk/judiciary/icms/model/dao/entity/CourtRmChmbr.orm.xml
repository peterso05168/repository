<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.CourtRmChmbr" access="FIELD">
     <table name="ccourt_rm_chmbr" />

     <attributes>
        <id name="courtRmChmbrId">
            <column name="court_rm_chmbr_id" />
            <generated-value strategy="TABLE" generator="CCOURT_RM_CHMBR_SEQ" />
            <table-generator name="CCOURT_RM_CHMBR_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="ccourt_rm_chmbr_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="name">
            <column name="name" />
        </basic>

        <basic name="courtRmCatType">
            <column name="court_rm_cat_type" />
        </basic>

        <basic name="fl">
            <column name="fl" />
        </basic>

        <basic name="status">
            <column name="status" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="courtVenue" fetch="LAZY">
        <join-column name="court_venue_id" referenced-column-name="court_venue_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
