<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.DocOutSignSeal" access="FIELD">
     <table name="tdoc_out_sign_seal" />

     <attributes>
        <id name="docOutSignAndSealId">
            <column name="doc_out_sign_and_seal_id" />
            <generated-value strategy="TABLE" generator="TDOC_OUT_SIGN_SEAL_SEQ" />
            <table-generator name="TDOC_OUT_SIGN_SEAL_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tdoc_out_sign_seal_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="signSealDate">
            <column name="sign_seal_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="signSealInd">
            <column name="sign_seal_ind" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="signSealByUserAc" fetch="LAZY">
        <join-column name="sign_seal_by_user_ac_id" referenced-column-name="intl_user_ac_id" />
    </many-to-one>

    <many-to-one name="doc" fetch="LAZY">
        <join-column name="doc_id" referenced-column-name="doc_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
