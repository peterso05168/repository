            <column name="drv_licence_disqual_ordr_id" />
            <generated-value strategy="TABLE" generator="TDRV_LICENCE_DISQUAL_ORDR_SEQ" />
            <table-generator name="TDRV_LICENCE_DISQUAL_ORDR_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tdrv_licence_disqual_ordr_id" 
