<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.ReprtRsn" access="FIELD">
     <table name="creprt_rsn" />

     <attributes>
        <id name="reprtRsnId">
            <column name="reprt_rsn_id" />
            <generated-value strategy="TABLE" generator="CREPRT_RSN_SEQ" />
            <table-generator name="CREPRT_RSN_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="creprt_rsn_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="reprtRsnCd">
            <column name="reprt_rsn_cd" />
        </basic>

        <basic name="reprtRsnDesc">
            <column name="reprt_rsn_desc" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->

<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
