<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.FpTxnSumy" access="FIELD">
     <table name="tfp_txn_sumy" />

     <attributes>
        <id name="fpTxnSumyId">
            <column name="fp_txn_sumy_id" />
            <generated-value strategy="TABLE" generator="TFP_TXN_SUMY_SEQ" />
            <table-generator name="TFP_TXN_SUMY_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tfp_txn_sumy_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="fpDailySumy" fetch="LAZY">
        <join-column name="fp_daily_sumy_id" referenced-column-name="fp_daily_sumy_id" />
    </many-to-one>

    <many-to-one name="fpTxn" fetch="LAZY">
        <join-column name="fp_txn_id" referenced-column-name="fp_txn_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
