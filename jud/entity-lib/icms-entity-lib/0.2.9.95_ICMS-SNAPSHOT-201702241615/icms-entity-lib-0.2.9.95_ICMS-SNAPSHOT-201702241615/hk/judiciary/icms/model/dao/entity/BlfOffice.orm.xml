<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.BlfOffice" access="FIELD">
     <table name="cblf_office" />

     <attributes>
        <id name="blfOfficeId">
            <column name="blf_office_id" />
            <generated-value strategy="TABLE" generator="CBLF_OFFICE_SEQ" />
            <table-generator name="CBLF_OFFICE_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="cblf_office_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="blfOfficeName">
            <column name="blf_office_name" />
        </basic>

        <basic name="blfOfficeGrp">
            <column name="blf_office_grp" />
        </basic>

        <basic name="blfOfficeCd">
            <column name="blf_office_cd" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="dfltGuardComp" fetch="LAZY">
        <join-column name="dflt_guard_comp_id" referenced-column-name="blf_secu_guard_comp_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
