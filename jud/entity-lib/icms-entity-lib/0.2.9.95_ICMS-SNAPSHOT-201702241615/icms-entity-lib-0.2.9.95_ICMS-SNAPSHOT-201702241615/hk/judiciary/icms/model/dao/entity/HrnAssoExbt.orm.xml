<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.HrnAssoExbt" access="FIELD">
     <table name="thrn_asso_exbt" />

     <attributes>
        <id name="hrnAssoExbtId">
            <column name="hrn_asso_exbt_id" />
            <generated-value strategy="TABLE" generator="THRN_ASSO_EXBT_SEQ" />
            <table-generator name="THRN_ASSO_EXBT_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="thrn_asso_exbt_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="exbtSubmitDate">
            <column name="exbt_submit_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="status">
            <column name="status" />
        </basic>

        <basic name="exbtRtnDate">
            <column name="exbt_rtn_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="nameOfRecip">
            <column name="name_of_recip" />
        </basic>

        <basic name="seq">
            <column name="seq" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="exbt" fetch="LAZY">
        <join-column name="exbt_id" referenced-column-name="exbt_id" />
    </many-to-one>

    <many-to-one name="hrnSchd" fetch="LAZY">
        <join-column name="hrn_schd_id" referenced-column-name="hrn_schd_id" />
    </many-to-one>

    <many-to-one name="rtnToPartcpRole" fetch="LAZY">
        <join-column name="rtn_to_partcp_role_id" referenced-column-name="partcp_role_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
