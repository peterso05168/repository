<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.PendIntlRpToFunc" access="FIELD">
     <table name="tpend_intl_rp_to_func" />

     <attributes>
        <id name="pendIntlRpToFuncId">
            <column name="pend_intl_rp_to_func_id" />
            <generated-value strategy="TABLE" generator="TPEND_INTL_RP_TO_FUNC_SEQ" />
            <table-generator name="TPEND_INTL_RP_TO_FUNC_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tpend_intl_rp_to_func_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="amendSeq">
            <column name="amend_seq" />
        </basic>

        <basic name="reqsDt">
            <column name="reqs_dt" />
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>

        <basic name="appvDt">
            <column name="appv_dt" />
        </basic>

        <basic name="appvStatus">
            <column name="appv_status" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="intlRpToFunc" fetch="LAZY">
        <join-column name="intl_rp_to_func_id" referenced-column-name="intl_rp_to_func_id" />
    </many-to-one>

    <many-to-one name="intlRp" fetch="LAZY">
        <join-column name="intl_rp_id" referenced-column-name="intl_rp_id" />
    </many-to-one>

    <many-to-one name="intlFunc" fetch="LAZY">
        <join-column name="intl_func_id" referenced-column-name="intl_func_id" />
    </many-to-one>

    <many-to-one name="appvBy" fetch="LAZY">
        <join-column name="appv_by" referenced-column-name="intl_user_ac_id" />
    </many-to-one>

    <many-to-one name="reqsBy" fetch="LAZY">
        <join-column name="reqs_by" referenced-column-name="intl_user_ac_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
