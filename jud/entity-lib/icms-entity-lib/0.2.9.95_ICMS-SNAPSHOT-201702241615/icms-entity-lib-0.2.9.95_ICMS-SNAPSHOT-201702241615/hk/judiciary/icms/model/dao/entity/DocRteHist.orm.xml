<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.DocRteHist" access="FIELD">
     <table name="tdoc_rte_hist" />

     <attributes>
        <id name="docRteHistId">
            <column name="doc_rte_hist_id" />
            <generated-value strategy="TABLE" generator="TDOC_RTE_HIST_SEQ" />
            <table-generator name="TDOC_RTE_HIST_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tdoc_rte_hist_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="docActRteStatus">
            <column name="doc_act_rte_status" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="inDoc" fetch="LAZY">
        <join-column name="in_doc_id" referenced-column-name="doc_id" />
    </many-to-one>

    <many-to-one name="jjoProfile" fetch="LAZY">
        <join-column name="jjo_profile_id" referenced-column-name="jjo_profile_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->
    <one-to-many name="docActAssign"  mapped-by="docRteHist" fetch="LAZY" />
    <one-to-many name="docStop"  mapped-by="docRteHist" fetch="LAZY" />
    <one-to-many name="dfltJudgtRslt"  mapped-by="docRteHist" fetch="LAZY" />
    <one-to-many name="dfOrdrRslt"  mapped-by="docRteHist" fetch="LAZY" />
    <one-to-many name="exparteAppRslt"  mapped-by="docRteHist" fetch="LAZY" />
    <one-to-many name="admisRslt"  mapped-by="docRteHist" fetch="LAZY" />

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
