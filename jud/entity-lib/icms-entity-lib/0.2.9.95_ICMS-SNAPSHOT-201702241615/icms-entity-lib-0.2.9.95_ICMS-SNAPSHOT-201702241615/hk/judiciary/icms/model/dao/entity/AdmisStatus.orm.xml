<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.AdmisStatus" access="FIELD">
     <table name="tadmis_status" />

     <attributes>
        <id name="admisStatusId">
            <column name="admis_status_id" />
            <generated-value strategy="TABLE" generator="TADMIS_STATUS_SEQ" />
            <table-generator name="TADMIS_STATUS_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tadmis_status_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="upliftRsn" fetch="LAZY">
        <join-column name="uplift_rsn_id" referenced-column-name="admis_uplift_rsn_id" />
    </many-to-one>

    <many-to-one name="upliftedBy" fetch="LAZY">
        <join-column name="uplifted_by" referenced-column-name="intl_user_ac_id" />
    </many-to-one>

    <many-to-one name="stayedBy" fetch="LAZY">
        <join-column name="stayed_by" referenced-column-name="intl_user_ac_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->
    <one-to-one name="stayOfProcsExec" fetch="LAZY">
        <join-column name="stay_of_procs_exec_id" referenced-column-name="stay_of_procs_exec_id" />
    </one-to-one>


<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
