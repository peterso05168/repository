<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.RepPosType" access="FIELD">
     <table name="crep_pos_type" />

     <attributes>
        <id name="posTypeId">
            <column name="pos_type_id" />
            <generated-value strategy="TABLE" generator="CREP_POS_TYPE_SEQ" />
            <table-generator name="CREP_POS_TYPE_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="crep_pos_type_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="seqNo">
            <column name="seq_no" />
        </basic>

        <basic name="posTypeCd">
            <column name="pos_type_cd" />
        </basic>

        <basic name="posTypeName">
            <column name="pos_type_name" />
        </basic>

        <basic name="posTypeChin">
            <column name="pos_type_chin" />
        </basic>

        <basic name="posTypeInd">
            <column name="pos_type_ind" />
        </basic>

        <basic name="posTypeChinSimpl">
            <column name="pos_type_chin_simpl" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->

<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
