<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.WarnedList" access="FIELD">
     <table name="twarned_list" />

     <attributes>
        <id name="warnedListId">
            <column name="warned_list_id" />
            <generated-value strategy="TABLE" generator="TWARNED_LIST_SEQ" />
            <table-generator name="TWARNED_LIST_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="twarned_list_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="warnedListWarnedDate">
            <column name="warned_list_warned_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="warnedPerdStartDate">
            <column name="warned_perd_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="warnedPerdEndDate">
            <column name="warned_perd_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="warnedListGenDt">
            <column name="warned_list_gen_dt" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="warnedListOtherRelevInfo">
            <column name="warned_list_other_relev_info" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
 <!--
    <many-to-one name="runList" fetch="LAZY">
        <join-column name="run_list_id" referenced-column-name="run_list_id" />
    </many-to-one>

 -->

    <many-to-one name="vcase" fetch="LAZY">
        <join-column name="case_id" referenced-column-name="case_id" />
    </many-to-one>

    <many-to-one name="warnedListReviewBy" fetch="LAZY">
        <join-column name="warned_list_review_by" referenced-column-name="intl_user_ac_id" />
    </many-to-one>

    <many-to-one name="warnedListAppvBy" fetch="LAZY">
        <join-column name="warned_list_appv_by" referenced-column-name="intl_user_ac_id" />
    </many-to-one>

    <many-to-one name="warnedListGenReqsBy" fetch="LAZY">
        <join-column name="warned_list_gen_reqs_by" referenced-column-name="intl_user_ac_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
