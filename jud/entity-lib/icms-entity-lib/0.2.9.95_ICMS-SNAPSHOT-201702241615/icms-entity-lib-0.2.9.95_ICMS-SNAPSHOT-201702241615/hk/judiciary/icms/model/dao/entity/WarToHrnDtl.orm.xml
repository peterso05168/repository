<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.WarToHrnDtl" access="FIELD">
     <table name="twar_to_hrn_dtl" />

     <attributes>
        <id name="warToHrnDtlId">
            <column name="war_to_hrn_dtl_id" />
            <generated-value strategy="TABLE" generator="TWAR_TO_HRN_DTL_SEQ" />
            <table-generator name="TWAR_TO_HRN_DTL_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="twar_to_hrn_dtl_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="war" fetch="LAZY">
        <join-column name="war_id" referenced-column-name="war_id" />
    </many-to-one>

    <many-to-one name="hrnDtl" fetch="LAZY">
        <join-column name="hrn_dtl_id" referenced-column-name="hrn_dtl_id" />
    </many-to-one>

    <many-to-one name="relshpType" fetch="LAZY">
        <join-column name="relshp_type_id" referenced-column-name="war_to_hrn_relshp_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
