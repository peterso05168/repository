<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.JcToJjo" access="FIELD">
     <table name="tjc_to_jjo" />

     <attributes>
        <id name="jcToJjoId">
            <column name="jc_to_jjo_id" />
            <generated-value strategy="TABLE" generator="TJC_TO_JJO_SEQ" />
            <table-generator name="TJC_TO_JJO_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tjc_to_jjo_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="jcToJjoEffvStartDate">
            <column name="jc_to_jjo_effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="jcToJjoEffvEndDate">
            <column name="jc_to_jjo_effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="jcToJjoStatus">
            <column name="jc_to_jjo_status" />
        </basic>

        <basic name="jcUserId">
            <column name="jc_user_id" />
        </basic>

        <basic name="edsAppvReq">
            <column name="eds_appv_req" />
        </basic>

        <basic name="supvrUserId">
            <column name="supvr_user_id" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="courtLvlType" fetch="LAZY">
        <join-column name="court_lvl_type_id" referenced-column-name="court_lvl_type_id" />
    </many-to-one>

    <many-to-one name="jjoProfile" fetch="LAZY">
        <join-column name="jjo_profile_id" referenced-column-name="jjo_profile_id" />
    </many-to-one>

    <many-to-one name="compsCourt" fetch="LAZY">
        <join-column name="comps_court_id" referenced-column-name="comps_court_id" />
    </many-to-one>

    <many-to-one name="courtRmChmbr" fetch="LAZY">
        <join-column name="court_rm_chmbr_id" referenced-column-name="court_rm_chmbr_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
