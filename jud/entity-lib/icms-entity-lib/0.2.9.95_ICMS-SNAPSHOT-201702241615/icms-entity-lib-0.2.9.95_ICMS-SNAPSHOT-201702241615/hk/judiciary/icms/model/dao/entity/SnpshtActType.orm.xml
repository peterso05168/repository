<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.SnpshtActType" access="FIELD">
     <table name="tsnpsht_act_type" />

     <attributes>
        <id name="snpshtActTypeId">
            <column name="snpsht_act_type_id" />
            <generated-value strategy="TABLE" generator="TSNPSHT_ACT_TYPE_SEQ" />
            <table-generator name="TSNPSHT_ACT_TYPE_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tsnpsht_act_type_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="snpshtActTypeCd">
            <column name="snpsht_act_type_cd" />
        </basic>

        <basic name="snpshtActTypeDesc">
            <column name="snpsht_act_type_desc" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->

<!-- Foreign key one-to-many -->
    <one-to-many name="snpshtAmendHist"  mapped-by="snpshtActType" fetch="LAZY" />

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
