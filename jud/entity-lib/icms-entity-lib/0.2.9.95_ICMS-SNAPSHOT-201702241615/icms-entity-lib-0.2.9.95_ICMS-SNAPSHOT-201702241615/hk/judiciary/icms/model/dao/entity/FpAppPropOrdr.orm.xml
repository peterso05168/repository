<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.FpAppPropOrdr" access="FIELD">
     <table name="tfp_app_prop_ordr" />

     <attributes>
        <id name="fpAppPropOrdrId">
            <column name="fp_app_prop_ordr_id" />
            <generated-value strategy="TABLE" generator="TFP_APP_PROP_ORDR_SEQ" />
            <table-generator name="TFP_APP_PROP_ORDR_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tfp_app_prop_ordr_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="fileCoverMemoDrn">
            <column name="file_cover_memo_drn" />
        </basic>

        <basic name="fpAmt">
            <column name="fp_amt" />
        </basic>

        <basic name="status">
            <column name="status" />
        </basic>

        <basic name="cost">
            <column name="cost" />
        </basic>

        <basic name="withdrawnRsn">
            <column name="withdrawn_rsn" />
        </basic>

        <basic name="fileCoverMemoInd">
            <column name="file_cover_memo_ind" />
        </basic>

        <basic name="vettedBy">
            <column name="vetted_by" />
        </basic>

        <basic name="confirmedByJjo">
            <column name="confirmed_by_jjo" />
        </basic>

        <basic name="addlAmt">
            <column name="addl_amt" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="fpAppPropOrdrType" fetch="LAZY">
        <join-column name="fp_app_prop_ordr_type_id" referenced-column-name="fp_app_prop_ordr_type_id" />
    </many-to-one>

    <many-to-one name="vcase" fetch="LAZY">
        <join-column name="case_id" referenced-column-name="case_id" />
    </many-to-one>

    <many-to-one name="fpAppPropOrdrGrp" fetch="LAZY">
        <join-column name="fp_app_prop_ordr_grp_id" referenced-column-name="fp_app_prop_ordr_grp_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
