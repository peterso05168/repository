<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.ExtRoleToFunc" access="FIELD">
     <table name="text_role_to_func" />

     <attributes>
        <id name="extRoleToFuncId">
            <column name="ext_role_to_func_id" />
            <generated-value strategy="TABLE" generator="TEXT_ROLE_TO_FUNC_SEQ" />
            <table-generator name="TEXT_ROLE_TO_FUNC_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="text_role_to_func_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="extRole" fetch="LAZY">
        <join-column name="ext_role_id" referenced-column-name="ext_role_id" />
    </many-to-one>

    <many-to-one name="extFunc" fetch="LAZY">
        <join-column name="ext_func_id" referenced-column-name="ext_func_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
