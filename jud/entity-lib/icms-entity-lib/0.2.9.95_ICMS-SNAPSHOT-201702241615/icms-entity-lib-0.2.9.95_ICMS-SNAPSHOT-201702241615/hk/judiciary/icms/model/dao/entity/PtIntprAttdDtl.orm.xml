<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.PtIntprAttdDtl" access="FIELD">
     <table name="tpt_intpr_attd_dtl" />

     <attributes>
        <id name="ptIntprAttdDtlId">
            <column name="pt_intpr_attd_dtl_id" />
            <generated-value strategy="TABLE" generator="TPT_INTPR_ATTD_DTL_SEQ" />
            <table-generator name="TPT_INTPR_ATTD_DTL_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tpt_intpr_attd_dtl_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="ptIntprRegNo">
            <column name="pt_intpr_reg_no" />
        </basic>

        <basic name="apprDate">
            <column name="appr_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="ptIntprStartTime">
            <column name="pt_intpr_start_time" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="ptIntprEndTime">
            <column name="pt_intpr_end_time" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="servProvFeeAmt">
            <column name="serv_prov_fee_amt" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="hrnDtl" fetch="LAZY">
        <join-column name="hrn_dtl_id" referenced-column-name="hrn_dtl_id" />
    </many-to-one>

    <many-to-one name="partcpRole" fetch="LAZY">
        <join-column name="partcp_role_id" referenced-column-name="partcp_role_id" />
    </many-to-one>

    <many-to-one name="hrnAttd" fetch="LAZY">
        <join-column name="hrn_attd_id" referenced-column-name="hrn_attd_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->
    <one-to-one name="ptIntprReqs" fetch="LAZY">
        <join-column name="pt_intpr_reqs_id" referenced-column-name="pt_intpr_reqs_id" />
    </one-to-one>


<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
