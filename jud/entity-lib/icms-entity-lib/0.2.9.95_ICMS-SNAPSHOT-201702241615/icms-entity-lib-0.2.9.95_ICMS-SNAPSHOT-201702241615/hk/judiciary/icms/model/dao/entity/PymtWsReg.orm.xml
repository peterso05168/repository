<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.PymtWsReg" access="FIELD">
     <table name="tpymt_ws_reg" />

     <attributes>
        <id name="pymtWsRegId">
            <column name="pymt_ws_reg_id" />
            <generated-value strategy="TABLE" generator="TPYMT_WS_REG_SEQ" />
            <table-generator name="TPYMT_WS_REG_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tpymt_ws_reg_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="chngBy">
            <column name="chng_by" />
        </basic>

        <basic name="chngDt">
            <column name="chng_dt" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="pymtWs" fetch="LAZY">
        <join-column name="pymt_ws_id" referenced-column-name="pymt_ws_id" />
    </many-to-one>

    <many-to-one name="pymtWsStatus" fetch="LAZY">
        <join-column name="pymt_ws_status_id" referenced-column-name="pymt_ws_status_id" />
    </many-to-one>

    <many-to-one name="pymtWsOprMode" fetch="LAZY">
        <join-column name="pymt_ws_opr_mode_id" referenced-column-name="pymt_ws_opr_mode_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
