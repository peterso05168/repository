        </basic>

        <basic name="licenceDepType">
            <column name="licence_dep_type" />
        </basic>

        <basic name="depWithinDay">
            <column name="dep_within_day" />
        </basic>

        <basic name="depWithinHr">
            <column name="dep_within_hr" />
        </basic>

        <basic name="depOnOrBefDate">
            <column name="dep_on_or_bef_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="startDate">
            <column name="start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="stnceOrdr" fetch="LAZY">
        <join-column name="stnce_ordr_id" referenced-column-name="stnce_ordr_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

