<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.CourtFacReqs" access="FIELD">
     <table name="tcourt_fac_reqs" />

     <attributes>
        <id name="courtFacReqsId">
            <column name="court_fac_reqs_id" />
            <generated-value strategy="TABLE" generator="TCOURT_FAC_REQS_SEQ" />
            <table-generator name="TCOURT_FAC_REQS_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tcourt_fac_reqs_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="courtFacReqsQty">
            <column name="court_fac_reqs_qty" />
        </basic>

        <basic name="status">
            <column name="status" />
        </basic>

        <basic name="dateOfRaiseCourtFacReqs">
            <column name="date_of_raise_court_fac_reqs" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="mcAppId">
            <column name="mc_app_id" />
        </basic>

        <basic name="reqsStartTime">
            <column name="reqs_start_time" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="estTimeLenMinute">
            <column name="est_time_len_minute" />
        </basic>

        <basic name="estTimeLenHr">
            <column name="est_time_len_hr" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="courtFac" fetch="LAZY">
        <join-column name="court_fac_id" referenced-column-name="court_fac_id" />
    </many-to-one>

    <many-to-one name="reqsByUser" fetch="LAZY">
        <join-column name="reqs_by_user_id" referenced-column-name="intl_user_ac_id" />
    </many-to-one>

    <many-to-one name="courtFacReqsGrp" fetch="LAZY">
        <join-column name="court_fac_reqs_grp_id" referenced-column-name="court_fac_reqs_grp_id" />
    </many-to-one>

    <many-to-one name="courtFacReqsStatus" fetch="LAZY">
        <join-column name="court_fac_reqs_status_id" referenced-column-name="court_fac_reqs_status_id" />
    </many-to-one>

    <many-to-one name="courtFacType" fetch="LAZY">
        <join-column name="court_fac_type_id" referenced-column-name="court_fac_type_id" />
    </many-to-one>

    <many-to-one name="hrnSchd" fetch="LAZY">
        <join-column name="hrn_schd_id" referenced-column-name="hrn_schd_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
