<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.HrnPty" access="FIELD">
     <table name="thrn_pty" />

     <attributes>
        <id name="hrnPtyId">
            <column name="hrn_pty_id" />
            <generated-value strategy="TABLE" generator="THRN_PTY_SEQ" />
            <table-generator name="THRN_PTY_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="thrn_pty_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="hrnDtl" fetch="LAZY">
        <join-column name="hrn_dtl_id" referenced-column-name="hrn_dtl_id" />
    </many-to-one>

    <many-to-one name="partcpRole" fetch="LAZY">
        <join-column name="partcp_role_id" referenced-column-name="partcp_role_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
