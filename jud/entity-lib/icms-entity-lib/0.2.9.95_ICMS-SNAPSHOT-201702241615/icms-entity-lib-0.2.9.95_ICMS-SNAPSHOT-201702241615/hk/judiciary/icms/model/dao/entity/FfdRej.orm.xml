<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.FfdRej" access="FIELD">
     <table name="tffd_rej" />

     <attributes>
        <id name="ffdRejId">
            <column name="ffd_rej_id" />
            <generated-value strategy="TABLE" generator="TFFD_REJ_SEQ" />
            <table-generator name="TFFD_REJ_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tffd_rej_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="ffd">
            <column name="ffd" />
            <temporal>TIMESTAMP</temporal>
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="vcase" fetch="LAZY">
        <join-column name="case_id" referenced-column-name="case_id" />
    </many-to-one>

    <many-to-one name="ffdRejRsn" fetch="LAZY">
        <join-column name="ffd_rej_rsn_id" referenced-column-name="ffd_rej_rsn_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
