<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.SysInfErr" access="FIELD">
     <table name="tsys_inf_err" />

     <attributes>
        <id name="sysInfErrId">
            <column name="sys_inf_err_id" />
            <generated-value strategy="TABLE" generator="TSYS_INF_ERR_SEQ" />
            <table-generator name="TSYS_INF_ERR_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tsys_inf_err_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="errMsg">
            <column name="err_msg" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="sysInfBat" fetch="LAZY">
        <join-column name="sys_inf_bat_id" referenced-column-name="sys_inf_bat_id" />
    </many-to-one>

    <many-to-one name="sysInfRec" fetch="LAZY">
        <join-column name="sys_inf_rec_id" referenced-column-name="sys_inf_rec_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
