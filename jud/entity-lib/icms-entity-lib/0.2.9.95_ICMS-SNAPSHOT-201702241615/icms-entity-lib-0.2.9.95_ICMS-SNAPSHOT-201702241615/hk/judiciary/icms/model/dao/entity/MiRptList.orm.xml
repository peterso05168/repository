<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.MiRptList" access="FIELD">
     <table name="cmi_rpt_list" />

     <attributes>
        <id name="miRptListId">
            <column name="mi_rpt_list_id" />
            <generated-value strategy="TABLE" generator="CMI_RPT_LIST_SEQ" />
            <table-generator name="CMI_RPT_LIST_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="cmi_rpt_list_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="miRptNo">
            <column name="mi_rpt_no" />
        </basic>

        <basic name="miRptName">
            <column name="mi_rpt_name" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="courtLvlType" fetch="LAZY">
        <join-column name="court_lvl_type_id" referenced-column-name="court_lvl_type_id" />
    </many-to-one>

    <many-to-one name="miRptCatType" fetch="LAZY">
        <join-column name="mi_rpt_cat_type_id" referenced-column-name="mi_rpt_cat_type_id" />
    </many-to-one>

    <many-to-one name="miRptPerdType" fetch="LAZY">
        <join-column name="mi_rpt_perd_type_id" referenced-column-name="mi_rpt_perd_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
