<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.FpChopOffDate" access="FIELD">
     <table name="tfp_chop_off_date" />

     <attributes>
        <id name="fpChopOffDateId">
            <column name="fp_chop_off_date_id" />
            <generated-value strategy="TABLE" generator="TFP_CHOP_OFF_DATE_SEQ" />
            <table-generator name="TFP_CHOP_OFF_DATE_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tfp_chop_off_date_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="chopOffDate">
            <column name="chop_off_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="pymtDate">
            <column name="pymt_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="tktDemandType" fetch="LAZY">
        <join-column name="tkt_demand_type_id" referenced-column-name="tkt_demand_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

