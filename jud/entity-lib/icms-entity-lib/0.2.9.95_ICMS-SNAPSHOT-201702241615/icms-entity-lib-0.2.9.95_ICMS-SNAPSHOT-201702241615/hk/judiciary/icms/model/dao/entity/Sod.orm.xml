<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.Sod" access="FIELD">
     <table name="csod" />

     <attributes>
        <id name="sodId">
            <column name="sod_id" />
            <generated-value strategy="TABLE" generator="CSOD_SEQ" />
            <table-generator name="CSOD_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="csod_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="chapNo">
            <column name="chap_no" />
        </basic>

        <basic name="subLeg">
            <column name="sub_leg" />
        </basic>

        <basic name="sectNo">
            <column name="sect_no" />
        </basic>

        <basic name="subsectNo">
            <column name="subsect_no" />
        </basic>

        <basic name="regulationNo">
            <column name="regulation_no" />
        </basic>

        <basic name="para">
            <column name="para" />
        </basic>

        <basic name="subRegulationNo">
            <column name="sub_regulation_no" />
        </basic>

        <basic name="subPara">
            <column name="sub_para" />
        </basic>

        <basic name="supplCd">
            <column name="suppl_cd" />
        </basic>

        <basic name="verNo">
            <column name="ver_no" />
        </basic>

        <basic name="ofncDesc">
            <column name="ofnc_desc" />
        </basic>

        <basic name="ofncDescChin">
            <column name="ofnc_desc_chin" />
        </basic>

        <basic name="contryToLaw">
            <column name="contry_to_law" />
        </basic>

        <basic name="contryToLawChin">
            <column name="contry_to_law_chin" />
        </basic>

        <basic name="chrgPartcrForSummon">
            <column name="chrg_partcr_for_summon" />
        </basic>

        <basic name="provisFlag">
            <column name="provis_flag" />
        </basic>

        <basic name="chrgPartcrChinForSummon">
            <column name="chrg_partcr_chin_for_summon" />
        </basic>

        <basic name="chrgPartcrForFpSummon">
            <column name="chrg_partcr_for_fp_summon" />
        </basic>

        <basic name="chrgPartcrChinForFpSummon">
            <column name="chrg_partcr_chin_for_fp_summon" />
        </basic>

        <basic name="chrgPartcrForFpNoti">
            <column name="chrg_partcr_for_fp_noti" />
        </basic>

        <basic name="chrgPartcrChinForFpNoti">
            <column name="chrg_partcr_chin_for_fp_noti" />
        </basic>

        <basic name="atthReqFlag">
            <column name="atth_req_flag" />
        </basic>

        <basic name="pgblAllowFlag">
            <column name="pgbl_allow_flag" />
        </basic>

        <basic name="timeBarOfnc">
            <column name="time_bar_ofnc" />
        </basic>

        <basic name="ownByCourtFlag">
            <column name="own_by_court_flag" />
        </basic>

        <basic name="timeBarDisc">
            <column name="time_bar_disc" />
        </basic>

        <basic name="applyToCcFlag">
            <column name="apply_to_cc_flag" />
        </basic>

        <basic name="applyToDnFlag">
            <column name="apply_to_dn_flag" />
        </basic>

        <basic name="applyToDopFlag">
            <column name="apply_to_dop_flag" />
        </basic>

        <basic name="applyToDsFlag">
            <column name="apply_to_ds_flag" />
        </basic>

        <basic name="applyToFpFlag">
            <column name="apply_to_fp_flag" />
        </basic>

        <basic name="dopIncur">
            <column name="dop_incur" />
        </basic>

        <basic name="fpReviewOrdrDesc">
            <column name="fp_review_ordr_desc" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="courtLvlType" fetch="LAZY">
        <join-column name="court_lvl_type_id" referenced-column-name="court_lvl_type_id" />
    </many-to-one>

    <many-to-one name="ofncClassCd" fetch="LAZY">
        <join-column name="ofnc_class_cd_id" referenced-column-name="ofnc_class_id" />
    </many-to-one>

    <many-to-one name="hrnTimeWgt" fetch="LAZY">
        <join-column name="hrn_time_wgt" referenced-column-name="hrn_time_wgt_id" />
    </many-to-one>

    <many-to-one name="pnltyWgt" fetch="LAZY">
        <join-column name="pnlty_wgt" referenced-column-name="pnlty_wgt_id" />
    </many-to-one>

    <many-to-one name="ownBy" fetch="LAZY">
        <join-column name="own_by" referenced-column-name="pd_id" />
    </many-to-one>

    <many-to-one name="fpAmt" fetch="LAZY">
        <join-column name="fp_amt" referenced-column-name="fp_amt_id" />
    </many-to-one>

    <many-to-one name="fpCost" fetch="LAZY">
        <join-column name="fp_cost" referenced-column-name="fp_cost_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->
    <one-to-many name="sodVar"  mapped-by="prntCd" fetch="LAZY" />
    <one-to-many name="sodPrivil"  mapped-by="sod" fetch="LAZY" />

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
