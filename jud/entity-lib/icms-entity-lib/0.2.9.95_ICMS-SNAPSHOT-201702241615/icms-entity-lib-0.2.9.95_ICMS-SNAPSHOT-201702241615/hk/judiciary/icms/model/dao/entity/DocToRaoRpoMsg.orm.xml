<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.DocToRaoRpoMsg" access="FIELD">
     <table name="tdoc_to_rao_rpo_msg" />

     <attributes>
        <id name="docToRaoRpoMsgId">
            <column name="doc_to_rao_rpo_msg_id" />
            <generated-value strategy="TABLE" generator="TDOC_TO_RAO_RPO_MSG_SEQ" />
            <table-generator name="TDOC_TO_RAO_RPO_MSG_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tdoc_to_rao_rpo_msg_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="raoRpoMsg" fetch="LAZY">
        <join-column name="rao_rpo_msg_id" referenced-column-name="rao_rpo_msg_id" />
    </many-to-one>

    <many-to-one name="doc" fetch="LAZY">
        <join-column name="doc_id" referenced-column-name="doc_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
