<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.IntlUserActingAppt" access="FIELD">
     <table name="tintl_user_acting_appt" />

     <attributes>
        <id name="intlUserActingApptId">
            <column name="intl_user_acting_appt_id" />
            <generated-value strategy="TABLE" generator="TINTL_USER_ACTING_APPT_SEQ" />
            <table-generator name="TINTL_USER_ACTING_APPT_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tintl_user_acting_appt_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="actingApptEndDate">
            <column name="acting_appt_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="actingApptStartDate">
            <column name="acting_appt_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="intlUserAc" fetch="LAZY">
        <join-column name="intl_user_ac_id" referenced-column-name="intl_user_ac_id" />
    </many-to-one>

    <many-to-one name="actingApptType" fetch="LAZY">
        <join-column name="acting_appt_type_id" referenced-column-name="acting_appt_type_id" />
    </many-to-one>

    <many-to-one name="actingIntlUserAc" fetch="LAZY">
        <join-column name="acting_intl_user_ac_id" referenced-column-name="intl_user_ac_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
