<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.HrnRsltDtl" access="FIELD">
     <table name="thrn_rslt_dtl" />

     <attributes>
        <id name="hrnRsltDtlId">
            <column name="hrn_rslt_dtl_id" />
            <generated-value strategy="TABLE" generator="THRN_RSLT_DTL_SEQ" />
            <table-generator name="THRN_RSLT_DTL_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="thrn_rslt_dtl_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="status">
            <column name="status" />
        </basic>

        <basic name="chrgReadInLangTypeInd">
            <column name="chrg_read_in_lang_type_ind" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="disqualRecType" fetch="LAZY">
        <join-column name="disqual_rec_type_id" referenced-column-name="disqual_rec_type_id" />
    </many-to-one>

    <many-to-one name="hrnRslt" fetch="LAZY">
        <join-column name="hrn_rslt_id" referenced-column-name="hrn_rslt_id" />
    </many-to-one>

    <many-to-one name="chrgToPartcp" fetch="LAZY">
        <join-column name="chrg_to_partcp_id" referenced-column-name="chrg_to_partcp_id" />
    </many-to-one>

    <many-to-one name="langType" fetch="LAZY">
        <join-column name="lang_type_id" referenced-column-name="lang_type_id" />
    </many-to-one>

    <many-to-one name="briefFactType" fetch="LAZY">
        <join-column name="brief_fact_type_id" referenced-column-name="brief_fact_type_id" />
    </many-to-one>

    <many-to-one name="pleaType" fetch="LAZY">
        <join-column name="plea_type_id" referenced-column-name="plea_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
