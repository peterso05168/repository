<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.DocPageHdrTmpl" access="FIELD">
     <table name="tdoc_page_hdr_tmpl" />

     <attributes>
        <id name="docPageHdrTmplId">
            <column name="doc_page_hdr_tmpl_id" />
            <generated-value strategy="TABLE" generator="TDOC_PAGE_HDR_TMPL_SEQ" />
            <table-generator name="TDOC_PAGE_HDR_TMPL_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tdoc_page_hdr_tmpl_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="hdrTmplName">
            <column name="hdr_tmpl_name" />
        </basic>

        <basic name="hdrTmplCd">
            <column name="hdr_tmpl_cd" />
        </basic>

        <basic name="hdrTmplDesc">
            <column name="hdr_tmpl_desc" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="compsCourt" fetch="LAZY">
        <join-column name="comps_court_id" referenced-column-name="comps_court_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->
    <one-to-many name="docPageHdrTmplDtl"  mapped-by="docPageHdrTmpl" fetch="LAZY" />
    <one-to-many name="docTmplDtl"  mapped-by="docPageHdrTmpl" fetch="LAZY" />

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
