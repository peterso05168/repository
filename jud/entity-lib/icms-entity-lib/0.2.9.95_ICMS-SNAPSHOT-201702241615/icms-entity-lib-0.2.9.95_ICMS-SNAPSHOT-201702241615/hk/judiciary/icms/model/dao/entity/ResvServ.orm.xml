<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.ResvServ" access="FIELD">
     <table name="tresv_serv" />

     <attributes>
        <id name="resvServId">
            <column name="resv_serv_id" />
            <generated-value strategy="TABLE" generator="TRESV_SERV_SEQ" />
            <table-generator name="TRESV_SERV_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tresv_serv_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="courtLvlType" fetch="LAZY">
        <join-column name="court_lvl_type_id" referenced-column-name="court_lvl_type_id" />
    </many-to-one>

    <many-to-one name="resvServType" fetch="LAZY">
        <join-column name="resv_serv_type_id" referenced-column-name="resv_serv_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->
    <one-to-many name="resvServCharcs"  mapped-by="resvServ" fetch="LAZY" />
    <one-to-many name="resvServSchd"  mapped-by="resvServ" fetch="LAZY" />

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
