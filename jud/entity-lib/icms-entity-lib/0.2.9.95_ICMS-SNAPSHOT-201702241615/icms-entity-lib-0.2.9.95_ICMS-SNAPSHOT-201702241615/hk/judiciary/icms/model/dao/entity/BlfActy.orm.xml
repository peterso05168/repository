<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.BlfActy" access="FIELD">
     <table name="tblf_acty" />

     <attributes>
        <id name="blfActyId">
            <column name="blf_acty_id" />
            <generated-value strategy="TABLE" generator="TBLF_ACTY_SEQ" />
            <table-generator name="TBLF_ACTY_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tblf_acty_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="asgmntDate">
            <column name="asgmnt_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="actyType">
            <column name="acty_type" />
        </basic>

        <basic name="actyDt">
            <column name="acty_dt" />
            <temporal>TIMESTAMP</temporal>
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="intlUserAc" fetch="LAZY">
        <join-column name="intl_user_ac_id" referenced-column-name="intl_user_ac_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
