<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.VexsLitiBlkCourt" access="FIELD">
     <table name="tvexs_liti_blk_court" />

     <attributes>
        <id name="vexsLitiBlkCourtId">
            <column name="vexs_liti_blk_court_id" />
            <generated-value strategy="TABLE" generator="TVEXS_LITI_BLK_COURT_SEQ" />
            <table-generator name="TVEXS_LITI_BLK_COURT_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tvexs_liti_blk_court_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="courtIntlNo">
            <column name="court_intl_no" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="courtLvlType" fetch="LAZY">
        <join-column name="court_lvl_type_id" referenced-column-name="court_lvl_type_id" />
    </many-to-one>

    <many-to-one name="vexsLitiBlk" fetch="LAZY">
        <join-column name="vexs_liti_blk_id" referenced-column-name="vexs_liti_blk_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
