<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.ReqsRslt" access="FIELD">
     <table name="treqs_rslt" />

     <attributes>
        <id name="blfReqsRsltId">
            <column name="blf_reqs_rslt_id" />
            <generated-value strategy="TABLE" generator="TREQS_RSLT_SEQ" />
            <table-generator name="TREQS_RSLT_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="treqs_rslt_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="blfReqsRsltNote">
            <column name="blf_reqs_rslt_note" />
        </basic>

        <basic name="postDate">
            <column name="post_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="postalCd">
            <column name="postal_cd" />
        </basic>

        <basic name="servDate">
            <column name="serv_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="servRtnDate">
            <column name="serv_rtn_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="endorDate">
            <column name="endor_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="rsltRsn" fetch="LAZY">
        <join-column name="rslt_rsn_id" referenced-column-name="blf_task_rslt_rsn_id" />
    </many-to-one>

    <many-to-one name="rsltStatus" fetch="LAZY">
        <join-column name="rslt_status_id" referenced-column-name="blf_task_rslt_status_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->
    <one-to-one name="blfReqs" fetch="LAZY">
        <join-column name="blf_reqs_id" referenced-column-name="reqs_id" />
    </one-to-one>


<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
