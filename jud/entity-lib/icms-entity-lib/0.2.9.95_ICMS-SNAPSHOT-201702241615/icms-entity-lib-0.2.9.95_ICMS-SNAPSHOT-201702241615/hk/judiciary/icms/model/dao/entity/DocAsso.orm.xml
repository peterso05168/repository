<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.DocAsso" access="FIELD">
     <table name="tdoc_asso" />

     <attributes>
        <id name="docAssoId">
            <column name="doc_asso_id" />
            <generated-value strategy="TABLE" generator="TDOC_ASSO_SEQ" />
            <table-generator name="TDOC_ASSO_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tdoc_asso_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="relDoc" fetch="LAZY">
        <join-column name="rel_doc_id" referenced-column-name="doc_id" />
    </many-to-one>

    <many-to-one name="frDoc" fetch="LAZY">
        <join-column name="fr_doc_id" referenced-column-name="doc_id" />
    </many-to-one>

    <many-to-one name="docAssoType" fetch="LAZY">
        <join-column name="doc_asso_type_id" referenced-column-name="doc_asso_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
