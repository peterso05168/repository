<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.HeadsheetValueHist" access="FIELD">
     <table name="theadsheet_value_hist" />

     <attributes>
        <id name="headsheetValueHistId">
            <column name="headsheet_value_hist_id" />
            <generated-value strategy="TABLE" generator="THEADSHEET_VALUE_HIST_SEQ" />
            <table-generator name="THEADSHEET_VALUE_HIST_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="theadsheet_value_hist_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="status">
            <column name="status" />
        </basic>

        <basic name="fieldValue">
            <column name="field_value" />
        </basic>

        <basic name="fieldName">
            <column name="field_name" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="headsheet" fetch="LAZY">
        <join-column name="headsheet_id" referenced-column-name="headsheet_id" />
    </many-to-one>

    <many-to-one name="headsheetValue" fetch="LAZY">
        <join-column name="headsheet_value_id" referenced-column-name="headsheet_value_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
