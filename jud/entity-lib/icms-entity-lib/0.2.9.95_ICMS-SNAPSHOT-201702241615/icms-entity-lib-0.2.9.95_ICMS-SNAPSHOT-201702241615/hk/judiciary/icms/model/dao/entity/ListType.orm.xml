<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.ListType" access="FIELD">
     <table name="clist_type" />

     <attributes>
        <id name="listTypeId">
            <column name="list_type_id" />
            <generated-value strategy="TABLE" generator="CLIST_TYPE_SEQ" />
            <table-generator name="CLIST_TYPE_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="clist_type_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="listTypeDesc">
            <column name="list_type_desc" />
        </basic>

        <basic name="listTypeCat">
            <column name="list_type_cat" />
        </basic>

        <basic name="status">
            <column name="status" />
        </basic>

        <basic name="quotaListInd">
            <column name="quota_list_ind" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="courtLvlType" fetch="LAZY">
        <join-column name="court_lvl_type_id" referenced-column-name="court_lvl_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
