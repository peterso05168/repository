<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.IntlUap" access="FIELD">
     <table name="tintl_uap" />

     <attributes>
        <id name="intlUapId">
            <column name="intl_uap_id" />
            <generated-value strategy="TABLE" generator="TINTL_UAP_SEQ" />
            <table-generator name="TINTL_UAP_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tintl_uap_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="intlUserAc" fetch="LAZY">
        <join-column name="intl_user_ac_id" referenced-column-name="intl_user_ac_id" />
    </many-to-one>

    <many-to-one name="intlUserGrp" fetch="LAZY">
        <join-column name="intl_user_grp_id" referenced-column-name="intl_user_grp_id" />
    </many-to-one>

    <many-to-one name="compsCourt" fetch="LAZY">
        <join-column name="comps_court_id" referenced-column-name="comps_court_id" />
    </many-to-one>

    <many-to-one name="intlRole" fetch="LAZY">
        <join-column name="intl_role_id" referenced-column-name="intl_role_id" />
    </many-to-one>

    <many-to-one name="intlGrpSect" fetch="LAZY">
        <join-column name="intl_grp_sect_id" referenced-column-name="intl_grp_sect_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->
    <one-to-many name="pendIntlUap"  mapped-by="intlUap" fetch="LAZY" />
    <one-to-many name="intlUapToFunc"  mapped-by="intlUap" fetch="LAZY" />

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
