<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.DailyCauseList" access="FIELD">
     <table name="tdaily_cause_list" />

     <attributes>
        <id name="dailyCauseListId">
            <column name="daily_cause_list_id" />
            <generated-value strategy="TABLE" generator="TDAILY_CAUSE_LIST_SEQ" />
            <table-generator name="TDAILY_CAUSE_LIST_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tdaily_cause_list_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="dailyCauseListSeq">
            <column name="daily_cause_list_seq" />
        </basic>

        <basic name="dateOfDailyCauseList">
            <column name="date_of_daily_cause_list" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="dailyCauseListInfo">
            <column name="daily_cause_list_info" />
        </basic>

        <basic name="dailyCauseListGenDt">
            <column name="daily_cause_list_gen_dt" />
            <temporal>TIMESTAMP</temporal>
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="hrnSchd" fetch="LAZY">
        <join-column name="hrn_schd_id" referenced-column-name="hrn_schd_id" />
    </many-to-one>

    <many-to-one name="vcase" fetch="LAZY">
        <join-column name="case_id" referenced-column-name="case_id" />
    </many-to-one>

    <many-to-one name="dailyCauseListGenReqsBy" fetch="LAZY">
        <join-column name="daily_cause_list_gen_reqs_by" referenced-column-name="intl_user_ac_id" />
    </many-to-one>

    <many-to-one name="dailyCauseListReviewBy" fetch="LAZY">
        <join-column name="daily_cause_list_review_by" referenced-column-name="intl_user_ac_id" />
    </many-to-one>

    <many-to-one name="dailyCauseListAppvBy" fetch="LAZY">
        <join-column name="daily_cause_list_appv_by" referenced-column-name="intl_user_ac_id" />
    </many-to-one>

    <many-to-one name="dailyCauseListType" fetch="LAZY">
        <join-column name="daily_cause_list_type_id" referenced-column-name="daily_cause_list_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
