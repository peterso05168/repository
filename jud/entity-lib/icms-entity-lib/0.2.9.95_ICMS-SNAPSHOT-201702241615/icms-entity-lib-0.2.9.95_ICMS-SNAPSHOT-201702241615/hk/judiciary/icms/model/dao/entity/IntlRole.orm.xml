<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.IntlRole" access="FIELD">
     <table name="cintl_role" />

     <attributes>
        <id name="intlRoleId">
            <column name="intl_role_id" />
            <generated-value strategy="TABLE" generator="CINTL_ROLE_SEQ" />
            <table-generator name="CINTL_ROLE_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="cintl_role_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="intlRoleCd">
            <column name="intl_role_cd" />
        </basic>

        <basic name="intlRoleName">
            <column name="intl_role_name" />
        </basic>

        <basic name="intlRoleDesc">
            <column name="intl_role_desc" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->

<!-- Foreign key one-to-many -->
    <one-to-many name="roleToCourtSeal"  mapped-by="intlRole" fetch="LAZY" />
    <one-to-many name="userToFileMoveLoc"  mapped-by="intlRole" fetch="LAZY" />

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
