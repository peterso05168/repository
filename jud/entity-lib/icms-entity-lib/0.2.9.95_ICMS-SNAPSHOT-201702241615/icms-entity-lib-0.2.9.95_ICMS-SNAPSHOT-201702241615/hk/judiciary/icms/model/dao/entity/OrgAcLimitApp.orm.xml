<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.OrgAcLimitApp" access="FIELD">
     <table name="torg_ac_limit_app" />

     <attributes>
        <id name="orgAcLimitAppId">
            <column name="org_ac_limit_app_id" />
            <generated-value strategy="TABLE" generator="TORG_AC_LIMIT_APP_SEQ" />
            <table-generator name="TORG_AC_LIMIT_APP_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="torg_ac_limit_app_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="rsn">
            <column name="rsn" />
        </basic>

        <basic name="reqsMaxNoOfAc">
            <column name="reqs_max_no_of_ac" />
        </basic>

        <basic name="txnRefNo">
            <column name="txn_ref_no" />
        </basic>

        <basic name="submitDate">
            <column name="submit_date" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="appStatus" fetch="LAZY">
        <join-column name="app_status_id" referenced-column-name="app_status_id" />
    </many-to-one>

    <many-to-one name="ewpOrg" fetch="LAZY">
        <join-column name="ewp_org_id" referenced-column-name="ewp_org_id" />
    </many-to-one>

    <many-to-one name="acType" fetch="LAZY">
        <join-column name="ac_type_id" referenced-column-name="ac_type_id" />
    </many-to-one>

    <many-to-one name="submitByExtUserAc" fetch="LAZY">
        <join-column name="submit_by_ext_user_ac_id" referenced-column-name="ext_user_ac_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->
    <one-to-many name="orgAcLimitAppv"  mapped-by="orgAcLimitApp" fetch="LAZY" />

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
