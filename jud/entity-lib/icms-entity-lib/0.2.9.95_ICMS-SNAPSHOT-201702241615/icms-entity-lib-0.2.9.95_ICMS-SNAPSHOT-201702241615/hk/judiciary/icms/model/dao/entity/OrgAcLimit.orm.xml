<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.OrgAcLimit" access="FIELD">
     <table name="torg_ac_limit" />

     <attributes>
        <id name="orgAcLimitId">
            <column name="org_ac_limit_id" />
            <generated-value strategy="TABLE" generator="TORG_AC_LIMIT_SEQ" />
            <table-generator name="TORG_AC_LIMIT_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="torg_ac_limit_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="maxNoOfAc">
            <column name="max_no_of_ac" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="acType" fetch="LAZY">
        <join-column name="ac_type_id" referenced-column-name="ac_type_id" />
    </many-to-one>

    <many-to-one name="ewpOrg" fetch="LAZY">
        <join-column name="ewp_org_id" referenced-column-name="ewp_org_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
