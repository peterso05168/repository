<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.BlfAssignAvbl" access="FIELD">
     <table name="tblf_assign_avbl" />

     <attributes>
        <id name="blfAssignAvblId">
            <column name="blf_assign_avbl_id" />
            <generated-value strategy="TABLE" generator="TBLF_ASSIGN_AVBL_SEQ" />
            <table-generator name="TBLF_ASSIGN_AVBL_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tblf_assign_avbl_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="unavblDtFr">
            <column name="unavbl_dt_fr" />
        </basic>

        <basic name="unavblDesc">
            <column name="unavbl_desc" />
        </basic>

        <basic name="unavblDtTo">
            <column name="unavbl_dt_to" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="blfAssign" fetch="LAZY">
        <join-column name="blf_assign_id" referenced-column-name="blf_assign_id" />
    </many-to-one>

    <many-to-one name="unavblType" fetch="LAZY">
        <join-column name="unavbl_type_id" referenced-column-name="unavbl_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
