<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.DocFlrAcsRight" access="FIELD">
     <table name="tdoc_flr_acs_right" />

     <attributes>
        <id name="docFlrAcsRightId">
            <column name="doc_flr_acs_right_id" />
            <generated-value strategy="TABLE" generator="TDOC_FLR_ACS_RIGHT_SEQ" />
            <table-generator name="TDOC_FLR_ACS_RIGHT_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tdoc_flr_acs_right_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="userAcGrpAcsType" fetch="LAZY">
        <join-column name="user_ac_grp_acs_type_id" referenced-column-name="user_ac_grp_acs_type_id" />
    </many-to-one>

    <many-to-one name="docAcsType" fetch="LAZY">
        <join-column name="doc_acs_type_id" referenced-column-name="doc_acs_type_id" />
    </many-to-one>

    <many-to-one name="userAcsPrivil" fetch="LAZY">
        <join-column name="user_acs_privil_id" referenced-column-name="user_acs_privil_id" />
    </many-to-one>

    <many-to-one name="subFlr" fetch="LAZY">
        <join-column name="sub_flr_id" referenced-column-name="sub_flr_id" />
    </many-to-one>

    <many-to-one name="doc" fetch="LAZY">
        <join-column name="doc_id" referenced-column-name="doc_id" />
    </many-to-one>

    <many-to-one name="extUserAc" fetch="LAZY">
        <join-column name="ext_user_ac_id" referenced-column-name="ext_user_ac_id" />
    </many-to-one>

    <many-to-one name="intlUserAc" fetch="LAZY">
        <join-column name="intl_user_ac_id" referenced-column-name="intl_user_ac_id" />
    </many-to-one>

    <many-to-one name="intlCgs" fetch="LAZY">
        <join-column name="intl_cgs_id" referenced-column-name="intl_cgs_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
