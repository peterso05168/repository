                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.PymtAdvSeq" access="FIELD">
     <table name="tpymt_adv_seq" />

     <attributes>
        <id name="pymtAdvSeqId">
            <column name="pymt_adv_seq_id" />
            <generated-value strategy="TABLE" generator="TPYMT_ADV_SEQ_SEQ" />
            <table-generator name="TPYMT_ADV_SEQ_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tpymt_adv_seq_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="pymtAdvSeqDate">
            <column name="pymt_adv_seq_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="pymtAdvSeqType">
            <column name="pymt_adv_seq_type" />
        </basic>

        <basic name="pymtAdvSeqNo">
            <column name="pymt_adv_seq_no" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


