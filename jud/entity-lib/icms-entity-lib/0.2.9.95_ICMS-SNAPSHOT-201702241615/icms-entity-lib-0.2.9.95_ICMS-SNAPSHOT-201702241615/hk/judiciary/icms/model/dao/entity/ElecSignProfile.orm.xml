<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.ElecSignProfile" access="FIELD">
     <table name="telec_sign_profile" />

     <attributes>
        <id name="elecSignProfileId">
            <column name="elec_sign_profile_id" />
            <generated-value strategy="TABLE" generator="TELEC_SIGN_PROFILE_SEQ" />
            <table-generator name="TELEC_SIGN_PROFILE_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="telec_sign_profile_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="reqPwdFlag">
            <column name="req_pwd_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="dfltElecSignType" fetch="LAZY">
        <join-column name="dflt_elec_sign_type_id" referenced-column-name="elec_sign_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->
    <one-to-one name="intlUserAc" fetch="LAZY">
        <join-column name="intl_user_ac_id" referenced-column-name="intl_user_ac_id" />
    </one-to-one>


<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
