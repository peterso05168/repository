<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.SodPrivil" access="FIELD">
     <table name="tsod_privil" />

     <attributes>
        <id name="sodPrivilId">
            <column name="sod_privil_id" />
            <generated-value strategy="TABLE" generator="TSOD_PRIVIL_SEQ" />
            <table-generator name="TSOD_PRIVIL_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tsod_privil_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="sod" fetch="LAZY">
        <join-column name="sod_id" referenced-column-name="sod_id" />
    </many-to-one>

    <many-to-one name="pd" fetch="LAZY">
        <join-column name="pd_id" referenced-column-name="pd_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
