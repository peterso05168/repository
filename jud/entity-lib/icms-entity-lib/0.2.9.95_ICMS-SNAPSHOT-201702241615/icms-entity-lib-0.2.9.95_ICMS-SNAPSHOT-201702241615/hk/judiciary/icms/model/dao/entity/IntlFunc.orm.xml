<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.IntlFunc" access="FIELD">
     <table name="tintl_func" />

     <attributes>
        <id name="intlFuncId">
            <column name="intl_func_id" />
            <generated-value strategy="TABLE" generator="TINTL_FUNC_SEQ" />
            <table-generator name="TINTL_FUNC_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tintl_func_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="intlFuncName">
            <column name="intl_func_name" />
        </basic>

        <basic name="intlFuncDesc">
            <column name="intl_func_desc" />
        </basic>

        <basic name="intlFuncCd">
            <column name="intl_func_cd" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="proj" fetch="LAZY">
        <join-column name="proj_id" referenced-column-name="proj_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->
    <one-to-many name="propTaskAct"  mapped-by="intlFunc" fetch="LAZY" />
    <one-to-many name="intlFuncGrpToFunc"  mapped-by="intlFunc" fetch="LAZY" />
    <one-to-many name="jjoFuncGrpToFunc"  mapped-by="intlFunc" fetch="LAZY" />
    <one-to-many name="jjoRoleToFunc"  mapped-by="intlFunc" fetch="LAZY" />
    <one-to-many name="jjoToRole"  mapped-by="intlFunc" fetch="LAZY" />
    <one-to-many name="wsToIntlFunc"  mapped-by="intlFunc" fetch="LAZY" />
 <!--
    <one-to-many name="fmkMenuGrpToFunc"  mapped-by="intlFunc" fetch="LAZY" />
 -->

    <one-to-many name="userDefdTaskAct"  mapped-by="intlFunc" fetch="LAZY" />
    <one-to-many name="intlRpToFunc"  mapped-by="intlFunc" fetch="LAZY" />
    <one-to-many name="pendIntlRpToFunc"  mapped-by="intlFunc" fetch="LAZY" />
    <one-to-many name="intlUapToFunc"  mapped-by="intlFunc" fetch="LAZY" />
    <one-to-many name="pendIntlUapToFunc"  mapped-by="intlFunc" fetch="LAZY" />

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
