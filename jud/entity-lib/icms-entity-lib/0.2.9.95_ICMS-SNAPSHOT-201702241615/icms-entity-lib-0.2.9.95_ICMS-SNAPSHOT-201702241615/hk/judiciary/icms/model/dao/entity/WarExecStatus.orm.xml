<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.WarExecStatus" access="FIELD">
     <table name="cwar_exec_status" />

     <attributes>
        <id name="warExecStatusId">
            <column name="war_exec_status_id" />
            <generated-value strategy="TABLE" generator="CWAR_EXEC_STATUS_SEQ" />
            <table-generator name="CWAR_EXEC_STATUS_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="cwar_exec_status_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="courtLvlTypeId">
            <column name="court_lvl_type_id" />
        </basic>

        <basic name="subType">
            <column name="sub_type" />
        </basic>

        <basic name="seqNo">
            <column name="seq_no" />
        </basic>

        <basic name="cd">
            <column name="cd" />
        </basic>

        <basic name="descEng">
            <column name="desc_eng" />
        </basic>

        <basic name="descChin">
            <column name="desc_chin" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->

<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
