<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.CourtSeal" access="FIELD">
     <table name="tcourt_seal" />

     <attributes>
        <id name="courtSealId">
            <column name="court_seal_id" />
            <generated-value strategy="TABLE" generator="TCOURT_SEAL_SEQ" />
            <table-generator name="TCOURT_SEAL_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tcourt_seal_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="effvStartDt">
            <column name="effv_start_dt" />
        </basic>

        <basic name="effvEndDt">
            <column name="effv_end_dt" />
        </basic>

        <basic name="path">
            <column name="path" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="courtSealType" fetch="LAZY">
        <join-column name="court_seal_type_id" referenced-column-name="court_seal_type_id" />
    </many-to-one>

    <many-to-one name="compsCourt" fetch="LAZY">
        <join-column name="comps_court_id" referenced-column-name="comps_court_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->
    <one-to-many name="affixDtl"  mapped-by="courtSeal" fetch="LAZY" />
    <one-to-many name="roleToCourtSeal"  mapped-by="courtSeal" fetch="LAZY" />

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
