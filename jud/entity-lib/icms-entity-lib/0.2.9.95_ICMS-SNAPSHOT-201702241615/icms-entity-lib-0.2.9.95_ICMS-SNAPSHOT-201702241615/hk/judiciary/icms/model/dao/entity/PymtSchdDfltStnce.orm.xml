        </basic>

        <basic name="imprisonmentYr">
            <column name="imprisonment_yr" />
        </basic>

        <basic name="imprisonmentMonth">
            <column name="imprisonment_month" />
        </basic>

        <basic name="imprisonmentWeek">
            <column name="imprisonment_week" />
        </basic>

        <basic name="imprisonmentDay">
            <column name="imprisonment_day" />
        </basic>

        <basic name="committalWarCourtSys">
            <column name="committal_war_court_sys" />
        </basic>

        <basic name="committalWarType">
            <column name="committal_war_type" />
        </basic>

        <basic name="committalWarYr">
            <column name="committal_war_yr" />
        </basic>

        <basic name="committalWarSerNo">
            <column name="committal_war_ser_no" />
        </basic>

        <basic name="issueDate">
            <column name="issue_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="cancelDate">
            <column name="cancel_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="otherDept" fetch="LAZY">
        <join-column name="other_dept" referenced-column-name="detain_centre_id" />
    </many-to-one>

    <many-to-one name="pymtSchd" fetch="LAZY">
        <join-column name="pymt_schd_id" referenced-column-name="pymt_schd_id" />
    </many-to-one>


