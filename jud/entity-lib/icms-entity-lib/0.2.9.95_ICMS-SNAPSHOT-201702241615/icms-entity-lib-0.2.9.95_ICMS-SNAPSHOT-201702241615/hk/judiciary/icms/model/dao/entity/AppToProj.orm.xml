<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.AppToProj" access="FIELD">
     <table name="tapp_to_proj" />

     <attributes>
        <id name="appToProjId">
            <column name="app_to_proj_id" />
            <generated-value strategy="TABLE" generator="TAPP_TO_PROJ_SEQ" />
            <table-generator name="TAPP_TO_PROJ_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tapp_to_proj_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="app" fetch="LAZY">
        <join-column name="app_id" referenced-column-name="app_id" />
    </many-to-one>

    <many-to-one name="proj" fetch="LAZY">
        <join-column name="proj_id" referenced-column-name="proj_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
