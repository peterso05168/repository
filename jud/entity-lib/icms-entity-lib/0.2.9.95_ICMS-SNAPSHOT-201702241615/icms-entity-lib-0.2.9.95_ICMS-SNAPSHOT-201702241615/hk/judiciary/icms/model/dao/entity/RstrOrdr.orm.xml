<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.RstrOrdr" access="FIELD">
     <table name="trstr_ordr" />

     <attributes>
        <id name="rstrOrdrId">
            <column name="rstr_ordr_id" />
            <generated-value strategy="TABLE" generator="TRSTR_ORDR_SEQ" />
            <table-generator name="TRSTR_ORDR_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="trstr_ordr_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="rstrOrdrSumy">
            <column name="rstr_ordr_sumy" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="ordr" fetch="LAZY">
        <join-column name="ordr_id" referenced-column-name="ordr_id" />
    </many-to-one>

    <many-to-one name="rstrOrdrType" fetch="LAZY">
        <join-column name="rstr_ordr_type_id" referenced-column-name="rstr_ordr_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
