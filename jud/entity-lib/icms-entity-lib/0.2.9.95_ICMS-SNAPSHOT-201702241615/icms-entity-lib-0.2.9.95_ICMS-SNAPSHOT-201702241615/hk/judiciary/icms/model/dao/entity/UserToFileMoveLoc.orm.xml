<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.UserToFileMoveLoc" access="FIELD">
     <table name="tuser_to_file_move_loc" />

     <attributes>
        <id name="userToFileMoveLocId">
            <column name="user_to_file_move_loc_id" />
            <generated-value strategy="TABLE" generator="TUSER_TO_FILE_MOVE_LOC_SEQ" />
            <table-generator name="TUSER_TO_FILE_MOVE_LOC_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tuser_to_file_move_loc_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="toDoFlag">
            <column name="to_do_flag" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="intlUserAc" fetch="LAZY">
        <join-column name="intl_user_ac_id" referenced-column-name="intl_user_ac_id" />
    </many-to-one>

    <many-to-one name="intlRole" fetch="LAZY">
        <join-column name="intl_role_id" referenced-column-name="intl_role_id" />
    </many-to-one>

    <many-to-one name="fileMoveLoc" fetch="LAZY">
        <join-column name="file_move_loc_id" referenced-column-name="file_move_loc_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
