<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.ApptToIntlUser" access="FIELD">
     <table name="cappt_to_intl_user" />

     <attributes>
        <id name="apptToIntlUserId">
            <column name="appt_to_intl_user_id" />
            <generated-value strategy="TABLE" generator="CAPPT_TO_INTL_USER_SEQ" />
            <table-generator name="CAPPT_TO_INTL_USER_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="cappt_to_intl_user_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="acPriFlag">
            <column name="ac_pri_flag" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="apptType" fetch="LAZY">
        <join-column name="appt_type_id" referenced-column-name="appt_type_id" />
    </many-to-one>

    <many-to-one name="intlUserAc" fetch="LAZY">
        <join-column name="intl_user_ac_id" referenced-column-name="intl_user_ac_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
