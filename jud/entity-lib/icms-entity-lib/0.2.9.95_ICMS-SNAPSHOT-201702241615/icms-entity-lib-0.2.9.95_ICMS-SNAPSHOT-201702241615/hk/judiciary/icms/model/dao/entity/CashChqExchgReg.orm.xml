<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.CashChqExchgReg" access="FIELD">
     <table name="tcash_chq_exchg_reg" />

     <attributes>
        <id name="cashChqExchgRegId">
            <column name="cash_chq_exchg_reg_id" />
            <generated-value strategy="TABLE" generator="TCASH_CHQ_EXCHG_REG_SEQ" />
            <table-generator name="TCASH_CHQ_EXCHG_REG_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tcash_chq_exchg_reg_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="exchgDt">
            <column name="exchg_dt" />
        </basic>

        <basic name="chqNo">
            <column name="chq_no" />
        </basic>

        <basic name="amt">
            <column name="amt" />
        </basic>

        <basic name="clsgDate">
            <column name="clsg_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="clsgStatus">
            <column name="clsg_status" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="pymtWs" fetch="LAZY">
        <join-column name="pymt_ws_id" referenced-column-name="pymt_ws_id" />
    </many-to-one>

    <many-to-one name="cashChqExchgType" fetch="LAZY">
        <join-column name="cash_chq_exchg_type_id" referenced-column-name="cash_chq_exchg_type_id" />
    </many-to-one>

    <many-to-one name="centre" fetch="LAZY">
        <join-column name="centre_id" referenced-column-name="centre_id" />
    </many-to-one>

    <many-to-one name="pymtCatCd" fetch="LAZY">
        <join-column name="pymt_cat_cd_id" referenced-column-name="pymt_cat_cd_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
