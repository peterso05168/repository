<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.BlfSubDist" access="FIELD">
     <table name="cblf_sub_dist" />

     <attributes>
        <id name="blfSubDistId">
            <column name="blf_sub_dist_id" />
            <generated-value strategy="TABLE" generator="CBLF_SUB_DIST_SEQ" />
            <table-generator name="CBLF_SUB_DIST_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="cblf_sub_dist_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="seqNo">
            <column name="seq_no" />
        </basic>

        <basic name="blfSubDistCd">
            <column name="blf_sub_dist_cd" />
        </basic>

        <basic name="blfSubDistName">
            <column name="blf_sub_dist_name" />
        </basic>

        <basic name="blfSubDistNameChin">
            <column name="blf_sub_dist_name_chin" />
        </basic>

        <basic name="roadNoType">
            <column name="road_no_type" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="blfDist" fetch="LAZY">
        <join-column name="blf_dist_id" referenced-column-name="blf_dist_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
