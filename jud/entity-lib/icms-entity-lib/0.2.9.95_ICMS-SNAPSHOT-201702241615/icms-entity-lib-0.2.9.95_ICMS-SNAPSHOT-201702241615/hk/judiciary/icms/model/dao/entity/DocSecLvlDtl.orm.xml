        <join-column name="expert_rpt_type_id" referenced-column-name="expert_rpt_type_id" />
    </many-to-one>

    <many-to-one name="judgtType" fetch="LAZY">
        <join-column name="judgt_type_id" referenced-column-name="judgt_type_id" />
    </many-to-one>

    <many-to-one name="ordr" fetch="LAZY">
        <join-column name="ordr_id" referenced-column-name="ordr_id" />
    </many-to-one>

    <many-to-one name="chrgOrGrsheOrdrType" fetch="LAZY">
        <join-column name="chrg_or_grshe_ordr_type_id" referenced-column-name="chrg_or_grshe_ordr_type_id" />
