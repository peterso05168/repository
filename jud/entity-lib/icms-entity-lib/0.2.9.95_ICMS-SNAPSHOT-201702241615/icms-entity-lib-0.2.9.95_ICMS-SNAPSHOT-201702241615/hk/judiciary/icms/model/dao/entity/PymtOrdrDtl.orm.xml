<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.PymtOrdrDtl" access="FIELD">
     <table name="tpymt_ordr_dtl" />

     <attributes>
        <id name="pymtOrdrDtlId">
            <column name="pymt_ordr_dtl_id" />
            <generated-value strategy="TABLE" generator="TPYMT_ORDR_DTL_SEQ" />
            <table-generator name="TPYMT_ORDR_DTL_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tpymt_ordr_dtl_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="pymtOrdrDtlDueDate">
            <column name="pymt_ordr_dtl_due_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="pymtOrdrDtlAmt">
            <column name="pymt_ordr_dtl_amt" />
        </basic>

        <basic name="seqNo">
            <column name="seq_no" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="pymtOrdrSumy" fetch="LAZY">
        <join-column name="pymt_ordr_sumy_id" referenced-column-name="pymt_ordr_sumy_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
