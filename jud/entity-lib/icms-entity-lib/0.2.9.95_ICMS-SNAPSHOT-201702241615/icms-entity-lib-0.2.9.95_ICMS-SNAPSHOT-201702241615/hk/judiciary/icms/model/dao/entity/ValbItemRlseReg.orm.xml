<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.ValbItemRlseReg" access="FIELD">
     <table name="tvalb_item_rlse_reg" />

     <attributes>
        <id name="valbItemRlseRegId">
            <column name="valb_item_rlse_reg_id" />
            <generated-value strategy="TABLE" generator="TVALB_ITEM_RLSE_REG_SEQ" />
            <table-generator name="TVALB_ITEM_RLSE_REG_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tvalb_item_rlse_reg_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="valbItemRlseRegItem">
            <column name="valb_item_rlse_reg_item" />
        </basic>

        <basic name="valbItemRlseRegDesc">
            <column name="valb_item_rlse_reg_desc" />
        </basic>

        <basic name="valbItemRegRlseDate">
            <column name="valb_item_reg_rlse_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="vcase" fetch="LAZY">
        <join-column name="case_id" referenced-column-name="case_id" />
    </many-to-one>

    <many-to-one name="valbItemRecvReg" fetch="LAZY">
        <join-column name="valb_item_recv_reg_id" referenced-column-name="valb_item_recv_reg_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->
    <one-to-many name="valbItemVochReg"  mapped-by="valbItemRlseReg" fetch="LAZY" />

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
