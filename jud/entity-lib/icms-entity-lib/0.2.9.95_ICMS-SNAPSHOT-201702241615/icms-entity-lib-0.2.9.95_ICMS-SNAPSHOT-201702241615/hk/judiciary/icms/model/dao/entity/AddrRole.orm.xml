<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.AddrRole" access="FIELD">
     <table name="caddr_role" />

     <attributes>
        <id name="addrRoleId">
            <column name="addr_role_id" />
            <generated-value strategy="TABLE" generator="CADDR_ROLE_SEQ" />
            <table-generator name="CADDR_ROLE_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="caddr_role_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="effvDate">
            <column name="effv_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="addrSeqNo">
            <column name="addr_seq_no" />
        </basic>

        <basic name="postalServFlag">
            <column name="postal_serv_flag" />
        </basic>

        <basic name="personServFlag">
            <column name="person_serv_flag" />
        </basic>

        <basic name="execServFlag">
            <column name="exec_serv_flag" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="partcp" fetch="LAZY">
        <join-column name="partcp_id" referenced-column-name="partcp_id" />
    </many-to-one>

    <many-to-one name="addr" fetch="LAZY">
        <join-column name="addr_id" referenced-column-name="addr_id" />
    </many-to-one>

    <many-to-one name="addrRoleType" fetch="LAZY">
        <join-column name="addr_role_type_id" referenced-column-name="addr_role_type_id" />
    </many-to-one>

    <many-to-one name="courtVenue" fetch="LAZY">
        <join-column name="court_venue_id" referenced-column-name="court_venue_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
