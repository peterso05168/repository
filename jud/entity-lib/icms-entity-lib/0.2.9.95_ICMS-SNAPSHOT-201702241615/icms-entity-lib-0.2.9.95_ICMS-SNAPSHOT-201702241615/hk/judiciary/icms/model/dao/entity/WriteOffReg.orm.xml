<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.WriteOffReg" access="FIELD">
     <table name="twrite_off_reg" />

     <attributes>
        <id name="writeOffRegId">
            <column name="write_off_reg_id" />
            <generated-value strategy="TABLE" generator="TWRITE_OFF_REG_SEQ" />
            <table-generator name="TWRITE_OFF_REG_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="twrite_off_reg_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="writeOffDt">
            <column name="write_off_dt" />
        </basic>

        <basic name="batNo">
            <column name="bat_no" />
        </basic>

        <basic name="cutOffDateFr">
            <column name="cut_off_date_fr" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="cutOffDateTo">
            <column name="cut_off_date_to" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="batDelDt">
            <column name="bat_del_dt" />
        </basic>

        <basic name="batCreateDt">
            <column name="bat_create_dt" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="writeOffStatus" fetch="LAZY">
        <join-column name="write_off_status_id" referenced-column-name="write_off_status_id" />
    </many-to-one>

    <many-to-one name="compsCourt" fetch="LAZY">
        <join-column name="comps_court_id" referenced-column-name="comps_court_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->
    <one-to-many name="writeOffRegItem"  mapped-by="writeOffReg" fetch="LAZY" />

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
