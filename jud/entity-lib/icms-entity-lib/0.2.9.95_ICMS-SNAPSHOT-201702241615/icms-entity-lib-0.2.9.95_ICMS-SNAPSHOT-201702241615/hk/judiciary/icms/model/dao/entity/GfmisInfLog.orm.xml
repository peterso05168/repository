<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.GfmisInfLog" access="FIELD">
     <table name="tgfmis_inf_log" />

     <attributes>
        <id name="gfmisItfLogId">
            <column name="gfmis_itf_log_id" />
            <generated-value strategy="TABLE" generator="TGFMIS_INF_LOG_SEQ" />
            <table-generator name="TGFMIS_INF_LOG_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tgfmis_inf_log_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="clsgDate">
            <column name="clsg_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="seq">
            <column name="seq" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="centre" fetch="LAZY">
        <join-column name="centre_id" referenced-column-name="centre_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
