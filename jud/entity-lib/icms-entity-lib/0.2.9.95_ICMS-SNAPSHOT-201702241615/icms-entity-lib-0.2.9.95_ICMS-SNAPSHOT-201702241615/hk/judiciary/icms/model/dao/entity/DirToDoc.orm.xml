<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.DirToDoc" access="FIELD">
     <table name="tdir_to_doc" />

     <attributes>
        <id name="dirToDocId">
            <column name="dir_to_doc_id" />
            <generated-value strategy="TABLE" generator="TDIR_TO_DOC_SEQ" />
            <table-generator name="TDIR_TO_DOC_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tdir_to_doc_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="dir" fetch="LAZY">
        <join-column name="dir_id" referenced-column-name="dir_id" />
    </many-to-one>

    <many-to-one name="docIdOfTheDirEcf" fetch="LAZY">
        <join-column name="doc_id_of_the_dir_ecf" referenced-column-name="doc_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
