<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.ExtWebPtalOrgBrch" access="FIELD">
     <table name="text_web_ptal_org_brch" />

     <attributes>
        <id name="ewpOrgBrchId">
            <column name="ewp_org_brch_id" />
            <generated-value strategy="TABLE" generator="TEXT_WEB_PTAL_ORG_BRCH_SEQ" />
            <table-generator name="TEXT_WEB_PTAL_ORG_BRCH_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="text_web_ptal_org_brch_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="brchName">
            <column name="brch_name" />
        </basic>

        <basic name="brchAddlInfo">
            <column name="brch_addl_info" />
        </basic>

        <basic name="hqFlag">
            <column name="hq_flag" />
        </basic>

        <basic name="telNo">
            <column name="tel_no" />
        </basic>

        <basic name="faxNo">
            <column name="fax_no" />
        </basic>

        <basic name="brchStatus">
            <column name="brch_status" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="addrRole" fetch="LAZY">
        <join-column name="addr_role_id" referenced-column-name="addr_role_id" />
    </many-to-one>

    <many-to-one name="ewpOrg" fetch="LAZY">
        <join-column name="ewp_org_id" referenced-column-name="ewp_org_id" />
    </many-to-one>

    <many-to-one name="lglFirmBrch" fetch="LAZY">
        <join-column name="lgl_firm_brch_id" referenced-column-name="lgl_firm_brch_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
