<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.AdmisOrdrMade" access="FIELD">
     <table name="tadmis_ordr_made" />

     <attributes>
        <id name="admisOrdrMadeId">
            <column name="admis_ordr_made_id" />
            <generated-value strategy="TABLE" generator="TADMIS_ORDR_MADE_SEQ" />
            <table-generator name="TADMIS_ORDR_MADE_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tadmis_ordr_made_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="admisOrdr" fetch="LAZY">
        <join-column name="admis_ordr_id" referenced-column-name="admis_ordr_id" />
    </many-to-one>

    <many-to-one name="admisRslt" fetch="LAZY">
        <join-column name="admis_rslt_id" referenced-column-name="admis_rslt_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
