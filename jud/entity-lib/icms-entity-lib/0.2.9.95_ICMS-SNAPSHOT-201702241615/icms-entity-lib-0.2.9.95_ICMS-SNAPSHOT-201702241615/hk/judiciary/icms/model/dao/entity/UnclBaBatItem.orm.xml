<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.UnclBaBatItem" access="FIELD">
     <table name="tuncl_ba_bat_item" />

     <attributes>
        <id name="unclBaBatItemId">
            <column name="uncl_ba_bat_item_id" />
            <generated-value strategy="TABLE" generator="TUNCL_BA_BAT_ITEM_SEQ" />
            <table-generator name="TUNCL_BA_BAT_ITEM_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tuncl_ba_bat_item_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="baRegTrnfAmt">
            <column name="ba_reg_trnf_amt" />
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="unclBaBat" fetch="LAZY">
        <join-column name="uncl_ba_bat_id" referenced-column-name="uncl_ba_bat_id" />
    </many-to-one>

    <many-to-one name="baReg" fetch="LAZY">
        <join-column name="ba_reg_id" referenced-column-name="ba_reg_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
