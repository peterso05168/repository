<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.PropTaskAct" access="FIELD">
     <table name="cprop_task_act" />

     <attributes>
        <id name="propTaskActId">
            <column name="prop_task_act_id" />
            <generated-value strategy="TABLE" generator="CPROP_TASK_ACT_SEQ" />
            <table-generator name="CPROP_TASK_ACT_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="cprop_task_act_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="ableToSelTmplFlag">
            <column name="able_to_sel_tmpl_flag" />
        </basic>

        <basic name="webservFlag">
            <column name="webserv_flag" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="intlFunc" fetch="LAZY">
        <join-column name="intl_func_id" referenced-column-name="intl_func_id" />
    </many-to-one>

    <many-to-one name="docTypePropStop" fetch="LAZY">
        <join-column name="doc_type_prop_stop_id" referenced-column-name="doc_type_prop_stop_id" />
    </many-to-one>

    <many-to-one name="propTaskActItem" fetch="LAZY">
        <join-column name="prop_task_act_item_id" referenced-column-name="prop_task_act_item_id" />
    </many-to-one>

    <many-to-one name="propTaskActType" fetch="LAZY">
        <join-column name="prop_task_act_type_id" referenced-column-name="prop_task_act_type_id" />
    </many-to-one>

    <many-to-one name="proj" fetch="LAZY">
        <join-column name="proj_id" referenced-column-name="proj_id" />
    </many-to-one>

    <many-to-one name="webservActType" fetch="LAZY">
        <join-column name="webserv_act_type_id" referenced-column-name="webserv_act_type_id" />
    </many-to-one>

    <many-to-one name="expectOutpDocType" fetch="LAZY">
        <join-column name="expect_outp_doc_type_id" referenced-column-name="expect_outp_doc_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
