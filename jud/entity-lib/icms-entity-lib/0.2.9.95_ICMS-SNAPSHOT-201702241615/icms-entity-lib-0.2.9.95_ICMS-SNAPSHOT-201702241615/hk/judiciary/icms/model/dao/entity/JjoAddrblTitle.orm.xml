<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.JjoAddrblTitle" access="FIELD">
     <table name="tjjo_addrbl_title" />

     <attributes>
        <id name="jjoAddrblTitleId">
            <column name="jjo_addrbl_title_id" />
            <generated-value strategy="TABLE" generator="TJJO_ADDRBL_TITLE_SEQ" />
            <table-generator name="TJJO_ADDRBL_TITLE_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tjjo_addrbl_title_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="status">
            <column name="status" />
        </basic>

        <basic name="addrblTitleTxt">
            <column name="addrbl_title_txt" />
        </basic>

        <basic name="addrblTitleType">
            <column name="addrbl_title_type" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="jjoProfile" fetch="LAZY">
        <join-column name="jjo_profile_id" referenced-column-name="jjo_profile_id" />
    </many-to-one>

    <many-to-one name="tmpl" fetch="LAZY">
        <join-column name="tmpl_id" referenced-column-name="tmpl_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
