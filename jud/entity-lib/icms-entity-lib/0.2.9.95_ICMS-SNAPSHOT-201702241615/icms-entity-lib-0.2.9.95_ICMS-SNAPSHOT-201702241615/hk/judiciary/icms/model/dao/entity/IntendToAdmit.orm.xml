<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.IntendToAdmit" access="FIELD">
     <table name="tintend_to_admit" />

     <attributes>
        <id name="intendToAdmitId">
            <column name="intend_to_admit_id" />
            <generated-value strategy="TABLE" generator="TINTEND_TO_ADMIT_SEQ" />
            <table-generator name="TINTEND_TO_ADMIT_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tintend_to_admit_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="intendToAdmitFlag">
            <column name="intend_to_admit_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="partcpRole" fetch="LAZY">
        <join-column name="partcp_role_id" referenced-column-name="partcp_role_id" />
    </many-to-one>

    <many-to-one name="docSecLvlDtl" fetch="LAZY">
        <join-column name="doc_sec_lvl_dtl_id" referenced-column-name="doc_sec_lvl_dtl_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
