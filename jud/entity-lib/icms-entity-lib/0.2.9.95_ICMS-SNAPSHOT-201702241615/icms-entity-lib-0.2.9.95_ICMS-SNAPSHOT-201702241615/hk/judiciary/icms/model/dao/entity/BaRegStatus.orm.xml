<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.BaRegStatus" access="FIELD">
     <table name="cba_reg_status" />

     <attributes>
        <id name="baRegStatusId">
            <column name="ba_reg_status_id" />
            <generated-value strategy="TABLE" generator="CBA_REG_STATUS_SEQ" />
            <table-generator name="CBA_REG_STATUS_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="cba_reg_status_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="baRegStatusCd">
            <column name="ba_reg_status_cd" />
        </basic>

        <basic name="baRegStatusDesc">
            <column name="ba_reg_status_desc" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->

<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
