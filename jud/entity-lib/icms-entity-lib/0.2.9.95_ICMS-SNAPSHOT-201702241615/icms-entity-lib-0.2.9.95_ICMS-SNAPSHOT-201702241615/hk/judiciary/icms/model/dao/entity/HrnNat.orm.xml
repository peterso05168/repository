<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.HrnNat" access="FIELD">
     <table name="chrn_nat" />

     <attributes>
        <id name="hrnNatId">
            <column name="hrn_nat_id" />
            <generated-value strategy="TABLE" generator="CHRN_NAT_SEQ" />
            <table-generator name="CHRN_NAT_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="chrn_nat_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="hrnNatDescChin">
            <column name="hrn_nat_desc_chin" />
        </basic>

        <basic name="hrnNatDesc">
            <column name="hrn_nat_desc" />
        </basic>

        <basic name="hrnNatCd">
            <column name="hrn_nat_cd" />
        </basic>

        <basic name="status">
            <column name="status" />
        </basic>

        <basic name="subRule">
            <column name="sub_rule" />
        </basic>

        <basic name="chap">
            <column name="chap" />
        </basic>

        <basic name="sect">
            <column name="sect" />
        </basic>

        <basic name="subSect">
            <column name="sub_sect" />
        </basic>

        <basic name="ordr">
            <column name="ordr" />
        </basic>

        <basic name="rule">
            <column name="rule" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="caseType" fetch="LAZY">
        <join-column name="case_type_id" referenced-column-name="case_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
