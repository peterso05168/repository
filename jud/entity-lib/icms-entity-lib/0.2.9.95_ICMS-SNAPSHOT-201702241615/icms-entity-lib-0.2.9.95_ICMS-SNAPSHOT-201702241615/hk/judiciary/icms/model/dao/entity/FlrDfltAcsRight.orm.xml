<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.FlrDfltAcsRight" access="FIELD">
     <table name="tflr_dflt_acs_right" />

     <attributes>
        <id name="flrDfltAcsRightId">
            <column name="flr_dflt_acs_right_id" />
            <generated-value strategy="TABLE" generator="TFLR_DFLT_ACS_RIGHT_SEQ" />
            <table-generator name="TFLR_DFLT_ACS_RIGHT_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tflr_dflt_acs_right_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="acsFlag">
            <column name="acs_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="userAcsPrivil" fetch="LAZY">
        <join-column name="user_acs_privil_id" referenced-column-name="user_acs_privil_id" />
    </many-to-one>

    <many-to-one name="flr" fetch="LAZY">
        <join-column name="flr_id" referenced-column-name="flr_id" />
    </many-to-one>

    <many-to-one name="intlCgs" fetch="LAZY">
        <join-column name="intl_cgs_id" referenced-column-name="intl_cgs_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
