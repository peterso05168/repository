    <many-to-one name="reqsPymtType" fetch="LAZY">
        <join-column name="reqs_pymt_type_id" referenced-column-name="admis_pymt_type_id" />
    </many-to-one>

    <many-to-one name="partcpRole" fetch="LAZY">
        <join-column name="partcp_role_id" referenced-column-name="partcp_role_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->
    <one-to-many name="intendToAdmit"  mapped-by="docSecLvlDtl" fetch="LAZY" />

<!-- Foreign key one-to-one -->
    <one-to-one name="doc" fetch="LAZY">
        <join-column name="doc_id" referenced-column-name="doc_id" />
    </one-to-one>


<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
