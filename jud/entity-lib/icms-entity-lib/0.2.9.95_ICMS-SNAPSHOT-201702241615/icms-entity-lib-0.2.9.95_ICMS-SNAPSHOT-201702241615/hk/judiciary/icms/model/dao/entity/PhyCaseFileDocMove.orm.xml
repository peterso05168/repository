<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.PhyCaseFileDocMove" access="FIELD">
     <table name="tphy_case_file_doc_move" />

     <attributes>
        <id name="phyCaseFileDocMoveId">
            <column name="phy_case_file_doc_move_id" />
            <generated-value strategy="TABLE" generator="TPHY_CASE_FILE_DOC_MOVE_SEQ" />
            <table-generator name="TPHY_CASE_FILE_DOC_MOVE_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tphy_case_file_doc_move_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="phyCaseFileDocMoveDate">
            <column name="phy_case_file_doc_move_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="phyCaseFileDocSendDate">
            <column name="phy_case_file_doc_send_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="phyCaseFileDocReqs" fetch="LAZY">
        <join-column name="phy_case_file_doc_reqs_id" referenced-column-name="phy_case_file_doc_reqs_id" />
    </many-to-one>

    <many-to-one name="toFileMoveLoc" fetch="LAZY">
        <join-column name="to_file_move_loc_id" referenced-column-name="file_move_loc_id" />
    </many-to-one>

    <many-to-one name="docFolio" fetch="LAZY">
        <join-column name="doc_folio_id" referenced-column-name="doc_folio_id" />
    </many-to-one>

    <many-to-one name="intlUserAc" fetch="LAZY">
        <join-column name="intl_user_ac_id" referenced-column-name="intl_user_ac_id" />
    </many-to-one>

    <many-to-one name="fileMovePur" fetch="LAZY">
        <join-column name="file_move_pur_id" referenced-column-name="file_move_pur_id" />
    </many-to-one>

    <many-to-one name="frFileMoveLoc" fetch="LAZY">
        <join-column name="fr_file_move_loc_id" referenced-column-name="file_move_loc_id" />
    </many-to-one>

    <many-to-one name="doc" fetch="LAZY">
        <join-column name="doc_id" referenced-column-name="doc_id" />
    </many-to-one>

    <many-to-one name="vcase" fetch="LAZY">
        <join-column name="case_id" referenced-column-name="case_id" />
    </many-to-one>

    <many-to-one name="fileMoveMode" fetch="LAZY">
        <join-column name="file_move_mode_id" referenced-column-name="file_move_mode_id" />
    </many-to-one>

    <many-to-one name="docMoveStatus" fetch="LAZY">
        <join-column name="doc_move_status_id" referenced-column-name="doc_move_status_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
