<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.DocStopRqusDtl" access="FIELD">
     <table name="tdoc_stop_rqus_dtl" />

     <attributes>
        <id name="docStopRqusDtlId">
            <column name="doc_stop_rqus_dtl_id" />
            <generated-value strategy="TABLE" generator="TDOC_STOP_RQUS_DTL_SEQ" />
            <table-generator name="TDOC_STOP_RQUS_DTL_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tdoc_stop_rqus_dtl_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="rqusSeq">
            <column name="rqus_seq" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="docStop" fetch="LAZY">
        <join-column name="doc_stop_id" referenced-column-name="doc_stop_id" />
    </many-to-one>

    <many-to-one name="officerBeingAssign" fetch="LAZY">
        <join-column name="officer_being_assign" referenced-column-name="intl_user_ac_id" />
    </many-to-one>

    <many-to-one name="procGrpBeingAssign" fetch="LAZY">
        <join-column name="proc_grp_being_assign" referenced-column-name="proc_grp_id" />
    </many-to-one>

    <many-to-one name="doc" fetch="LAZY">
        <join-column name="doc_id" referenced-column-name="doc_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
