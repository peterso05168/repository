<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.EpayTxnccMap" access="FIELD">
     <table name="cepay_txncc_map" />

     <attributes>
        <id name="courtLvlCd">
            <column name="court_lvl_cd" />
            <generated-value strategy="TABLE" generator="CEPAY_TXNCC_MAP_SEQ" />
            <table-generator name="CEPAY_TXNCC_MAP_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="cepay_txncc_map_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="centre" fetch="LAZY">
        <join-column name="centre_id" referenced-column-name="centre_id" />
    </many-to-one>

    <many-to-one name="pymtWs" fetch="LAZY">
        <join-column name="pymt_ws_id" referenced-column-name="pymt_ws_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
