<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.JjoToList" access="FIELD">
     <table name="tjjo_to_list" />

     <attributes>
        <id name="jjoToListId">
            <column name="jjo_to_list_id" />
            <generated-value strategy="TABLE" generator="TJJO_TO_LIST_SEQ" />
            <table-generator name="TJJO_TO_LIST_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tjjo_to_list_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="supsdInd">
            <column name="supsd_ind" />
        </basic>

        <basic name="status">
            <column name="status" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="jjoToBeSupsd" fetch="LAZY">
        <join-column name="jjo_to_be_supsd" referenced-column-name="jjo_to_list_id" />
    </many-to-one>

    <many-to-one name="list" fetch="LAZY">
        <join-column name="list_id" referenced-column-name="list_id" />
    </many-to-one>

    <many-to-one name="jjoProfile" fetch="LAZY">
        <join-column name="jjo_profile_id" referenced-column-name="jjo_profile_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
