<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.DfltJudgtRslt" access="FIELD">
     <table name="tdflt_judgt_rslt" />

     <attributes>
        <id name="dfltJudgtRsltId">
            <column name="dflt_judgt_rslt_id" />
            <generated-value strategy="TABLE" generator="TDFLT_JUDGT_RSLT_SEQ" />
            <table-generator name="TDFLT_JUDGT_RSLT_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tdflt_judgt_rslt_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="otherRejRsn">
            <column name="other_rej_rsn" />
        </basic>

        <basic name="procDate">
            <column name="proc_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="otherQueryRsn">
            <column name="other_query_rsn" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="rejRsn" fetch="LAZY">
        <join-column name="rej_rsn_id" referenced-column-name="dflt_judgt_rej_rsn_id" />
    </many-to-one>

    <many-to-one name="docInfo" fetch="LAZY">
        <join-column name="doc_info_id" referenced-column-name="doc_id" />
    </many-to-one>

    <many-to-one name="outc" fetch="LAZY">
        <join-column name="outc_id" referenced-column-name="dflt_judgt_outc_id" />
    </many-to-one>

    <many-to-one name="docRteHist" fetch="LAZY">
        <join-column name="doc_rte_hist_id" referenced-column-name="doc_rte_hist_id" />
    </many-to-one>

    <many-to-one name="procBy" fetch="LAZY">
        <join-column name="proc_by" referenced-column-name="intl_user_ac_id" />
    </many-to-one>

    <many-to-one name="queryRsn" fetch="LAZY">
        <join-column name="query_rsn_id" referenced-column-name="dflt_judgt_query_rsn_id" />
    </many-to-one>

    <many-to-one name="appvStatus" fetch="LAZY">
        <join-column name="appv_status_id" referenced-column-name="dflt_judgt_appv_status_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
