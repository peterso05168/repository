<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.OrdrDate" access="FIELD">
     <table name="tordr_date" />

     <attributes>
        <id name="ordrDateId">
            <column name="ordr_date_id" />
            <generated-value strategy="TABLE" generator="TORDR_DATE_SEQ" />
            <table-generator name="TORDR_DATE_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tordr_date_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="dfOrdrDate">
            <column name="df_ordr_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="stnceOrdr" fetch="LAZY">
        <join-column name="stnce_ordr_id" referenced-column-name="stnce_ordr_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
