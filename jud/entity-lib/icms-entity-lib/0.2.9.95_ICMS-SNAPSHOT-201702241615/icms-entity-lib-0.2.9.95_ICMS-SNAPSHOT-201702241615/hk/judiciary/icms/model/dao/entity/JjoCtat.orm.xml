<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.JjoCtat" access="FIELD">
     <table name="tjjo_ctat" />

     <attributes>
        <id name="jjoCtatId">
            <column name="jjo_ctat_id" />
            <generated-value strategy="TABLE" generator="TJJO_CTAT_SEQ" />
            <table-generator name="TJJO_CTAT_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tjjo_ctat_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="faxNo">
            <column name="fax_no" />
        </basic>

        <basic name="mobiNo">
            <column name="mobi_no" />
        </basic>

        <basic name="telNo">
            <column name="tel_no" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="jjoAc" fetch="LAZY">
        <join-column name="jjo_ac_id" referenced-column-name="jjo_ac_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
