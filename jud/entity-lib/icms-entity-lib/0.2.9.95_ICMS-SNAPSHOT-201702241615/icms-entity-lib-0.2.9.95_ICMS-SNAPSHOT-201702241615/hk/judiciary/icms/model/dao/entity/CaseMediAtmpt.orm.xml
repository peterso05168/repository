<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.CaseMediAtmpt" access="FIELD">
     <table name="tcase_medi_atmpt" />

     <attributes>
        <id name="caseMediAtmptId">
            <column name="case_medi_atmpt_id" />
            <generated-value strategy="TABLE" generator="TCASE_MEDI_ATMPT_SEQ" />
            <table-generator name="TCASE_MEDI_ATMPT_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tcase_medi_atmpt_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="mediAtmptStartDate">
            <column name="medi_atmpt_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="mediSemDate">
            <column name="medi_sem_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="mediAtmptEndDate">
            <column name="medi_atmpt_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="vcase" fetch="LAZY">
        <join-column name="case_id" referenced-column-name="case_id" />
    </many-to-one>

    <many-to-one name="docIdOfMediMins" fetch="LAZY">
        <join-column name="doc_id_of_medi_mins" referenced-column-name="doc_id" />
    </many-to-one>

    <many-to-one name="docIdOfMediCert" fetch="LAZY">
        <join-column name="doc_id_of_medi_cert" referenced-column-name="doc_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
