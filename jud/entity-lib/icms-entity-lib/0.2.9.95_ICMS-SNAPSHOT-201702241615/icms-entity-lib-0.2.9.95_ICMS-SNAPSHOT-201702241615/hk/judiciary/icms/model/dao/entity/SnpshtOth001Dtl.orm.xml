        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="snpshtDtl" fetch="LAZY">
        <join-column name="snpsht_dtl_id" referenced-column-name="snpsht_dtl_id" />
    </many-to-one>

    <many-to-one name="trialDayGrp" fetch="LAZY">
        <join-column name="trial_day_grp_id" referenced-column-name="mi_rpt_param_id" />
    </many-to-one>


