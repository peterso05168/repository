            <table-generator name="TMHO_JJO_ROSTER_RGN_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tmho_jjo_roster_rgn_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="status">
