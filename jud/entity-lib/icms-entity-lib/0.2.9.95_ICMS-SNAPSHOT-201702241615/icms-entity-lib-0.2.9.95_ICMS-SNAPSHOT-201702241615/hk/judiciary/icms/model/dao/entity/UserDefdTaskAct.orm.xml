<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.UserDefdTaskAct" access="FIELD">
     <table name="tuser_defd_task_act" />

     <attributes>
        <id name="userDefdTaskActId">
            <column name="user_defd_task_act_id" />
            <generated-value strategy="TABLE" generator="TUSER_DEFD_TASK_ACT_SEQ" />
            <table-generator name="TUSER_DEFD_TASK_ACT_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tuser_defd_task_act_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="ableToSelTmplFlag">
            <column name="able_to_sel_tmpl_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="intlFunc" fetch="LAZY">
        <join-column name="intl_func_id" referenced-column-name="intl_func_id" />
    </many-to-one>

    <many-to-one name="taskActType" fetch="LAZY">
        <join-column name="task_act_type_id" referenced-column-name="prop_task_act_type_id" />
    </many-to-one>

    <many-to-one name="docStopAct" fetch="LAZY">
        <join-column name="doc_stop_act_id" referenced-column-name="doc_stop_act_id" />
    </many-to-one>

    <many-to-one name="taskActItem" fetch="LAZY">
        <join-column name="task_act_item_id" referenced-column-name="prop_task_act_item_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->
    <one-to-many name="userDefdActToTmpl"  mapped-by="userDefdTaskAct" fetch="LAZY" />

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
