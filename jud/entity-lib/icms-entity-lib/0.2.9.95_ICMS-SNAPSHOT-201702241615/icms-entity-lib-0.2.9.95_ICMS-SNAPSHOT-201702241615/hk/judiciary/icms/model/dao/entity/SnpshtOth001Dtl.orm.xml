            <column name="snpsht_oth_001_dtl_id" />
            <generated-value strategy="TABLE" generator="TSNPSHT_OTH_001_DTL_SEQ" />
            <table-generator name="TSNPSHT_OTH_001_DTL_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tsnpsht_oth_001_dtl_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="noOfListedCases">
            <column name="no_of_listed_cases" />
        </basic>

        <basic name="noOfListedDay">
            <column name="no_of_listed_day" />
