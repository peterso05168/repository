<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.FpTktTxnHist" access="FIELD">
     <table name="tfp_tkt_txn_hist" />

     <attributes>
        <id name="fpTktTxnHistId">
            <column name="fp_tkt_txn_hist_id" />
            <generated-value strategy="TABLE" generator="TFP_TKT_TXN_HIST_SEQ" />
            <table-generator name="TFP_TKT_TXN_HIST_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tfp_tkt_txn_hist_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="fpTkt" fetch="LAZY">
        <join-column name="fp_tkt_id" referenced-column-name="fp_tkt_id" />
    </many-to-one>

    <many-to-one name="fpTxn" fetch="LAZY">
        <join-column name="fp_txn_id" referenced-column-name="fp_txn_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
