<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.DocOutRecip" access="FIELD">
     <table name="tdoc_out_recip" />

     <attributes>
        <id name="docOutRecipId">
            <column name="doc_out_recip_id" />
            <generated-value strategy="TABLE" generator="TDOC_OUT_RECIP_SEQ" />
            <table-generator name="TDOC_OUT_RECIP_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tdoc_out_recip_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="dstrDate">
            <column name="dstr_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="doc" fetch="LAZY">
        <join-column name="doc_id" referenced-column-name="doc_id" />
    </many-to-one>

    <many-to-one name="partcpRole" fetch="LAZY">
        <join-column name="partcp_role_id" referenced-column-name="partcp_role_id" />
    </many-to-one>

    <many-to-one name="docCh" fetch="LAZY">
        <join-column name="doc_ch_id" referenced-column-name="doc_ch_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
