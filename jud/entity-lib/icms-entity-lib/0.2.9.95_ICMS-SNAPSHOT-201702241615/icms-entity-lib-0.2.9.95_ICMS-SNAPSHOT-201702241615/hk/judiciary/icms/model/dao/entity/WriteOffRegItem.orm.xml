<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.WriteOffRegItem" access="FIELD">
     <table name="twrite_off_reg_item" />

     <attributes>
        <id name="writeOffRegItemId">
            <column name="write_off_reg_item_id" />
            <generated-value strategy="TABLE" generator="TWRITE_OFF_REG_ITEM_SEQ" />
            <table-generator name="TWRITE_OFF_REG_ITEM_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="twrite_off_reg_item_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="pymtReqs" fetch="LAZY">
        <join-column name="pymt_reqs_id" referenced-column-name="pymt_reqs_id" />
    </many-to-one>

    <many-to-one name="writeOffReg" fetch="LAZY">
        <join-column name="write_off_reg_id" referenced-column-name="write_off_reg_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
