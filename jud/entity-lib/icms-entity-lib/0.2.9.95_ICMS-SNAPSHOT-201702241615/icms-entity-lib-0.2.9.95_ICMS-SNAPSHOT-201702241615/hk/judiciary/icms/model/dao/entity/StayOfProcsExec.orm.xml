<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.StayOfProcsExec" access="FIELD">
     <table name="tstay_of_procs_exec" />

     <attributes>
        <id name="stayOfProcsExecId">
            <column name="stay_of_procs_exec_id" />
            <generated-value strategy="TABLE" generator="TSTAY_OF_PROCS_EXEC_SEQ" />
            <table-generator name="TSTAY_OF_PROCS_EXEC_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tstay_of_procs_exec_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="stayOfProcsExecStartDate">
            <column name="stay_of_procs_exec_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="stayOfProcsExecEndDate">
            <column name="stay_of_procs_exec_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="stayOfProcsExecUpliftDate">
            <column name="stay_of_procs_exec_uplift_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="stayOfProcsExecStatus" fetch="LAZY">
        <join-column name="stay_of_procs_exec_status_id" referenced-column-name="stay_of_procs_exec_status_id" />
    </many-to-one>

    <many-to-one name="vcase" fetch="LAZY">
        <join-column name="case_id" referenced-column-name="case_id" />
    </many-to-one>

    <many-to-one name="stayOfProcsExecType" fetch="LAZY">
        <join-column name="stay_of_procs_exec_type_id" referenced-column-name="stay_of_procs_exec_type_id" />
    </many-to-one>

    <many-to-one name="extendOrUpliftOrdr" fetch="LAZY">
        <join-column name="extend_or_uplift_ordr_id" referenced-column-name="ordr_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->
    <one-to-one name="admisStatus"  mapped-by="stayOfProcsExec" fetch="LAZY" />

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
