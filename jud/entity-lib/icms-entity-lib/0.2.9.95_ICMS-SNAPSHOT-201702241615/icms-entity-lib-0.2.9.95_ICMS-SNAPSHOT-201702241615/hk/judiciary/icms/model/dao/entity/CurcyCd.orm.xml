<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.CurcyCd" access="FIELD">
     <table name="ccurcy_cd" />

     <attributes>
        <id name="curcyCdId">
            <column name="curcy_cd_id" />
            <generated-value strategy="TABLE" generator="CCURCY_CD_SEQ" />
            <table-generator name="CCURCY_CD_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="ccurcy_cd_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="curcyCd">
            <column name="curcy_cd" />
        </basic>

        <basic name="curcyName">
            <column name="curcy_name" />
        </basic>

        <basic name="curcyNameChin">
            <column name="curcy_name_chin" />
        </basic>

        <basic name="curcyEffvStartDate">
            <column name="curcy_effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="curcyEffvEndDate">
            <column name="curcy_effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="curcyStatus">
            <column name="curcy_status" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->

<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
