<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.RefunReg" access="FIELD">
     <table name="trefun_reg" />

     <attributes>
        <id name="refunRegId">
            <column name="refun_reg_id" />
            <generated-value strategy="TABLE" generator="TREFUN_REG_SEQ" />
            <table-generator name="TREFUN_REG_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="trefun_reg_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="refunDt">
            <column name="refun_dt" />
        </basic>

        <basic name="refunReqsDate">
            <column name="refun_reqs_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="refunPayOutCd">
            <column name="refun_pay_out_cd" />
        </basic>

        <basic name="refunChqNo">
            <column name="refun_chq_no" />
        </basic>

        <basic name="payeeName">
            <column name="payee_name" />
        </basic>

        <basic name="refunStatus">
            <column name="refun_status" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="txn" fetch="LAZY">
        <join-column name="txn_id" referenced-column-name="pymt_in_txn_id" />
    </many-to-one>

    <many-to-one name="doc" fetch="LAZY">
        <join-column name="doc_id" referenced-column-name="doc_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
