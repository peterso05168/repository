            <generated-value strategy="TABLE" generator="TEXT_USER_AC_TO_ROLE_SEQ" />
            <table-generator name="TEXT_USER_AC_TO_ROLE_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="text_user_ac_to_role_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
