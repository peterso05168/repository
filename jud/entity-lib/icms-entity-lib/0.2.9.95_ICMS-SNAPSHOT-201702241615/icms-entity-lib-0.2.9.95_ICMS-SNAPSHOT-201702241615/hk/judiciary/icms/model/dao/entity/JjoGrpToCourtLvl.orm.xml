<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.JjoGrpToCourtLvl" access="FIELD">
     <table name="tjjo_grp_to_court_lvl" />

     <attributes>
        <id name="jjoGrpToCourtLvlId">
            <column name="jjo_grp_to_court_lvl_id" />
            <generated-value strategy="TABLE" generator="TJJO_GRP_TO_COURT_LVL_SEQ" />
            <table-generator name="TJJO_GRP_TO_COURT_LVL_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tjjo_grp_to_court_lvl_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="courtLvlType" fetch="LAZY">
        <join-column name="court_lvl_type_id" referenced-column-name="court_lvl_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->
    <one-to-one name="jjoGrp" fetch="LAZY">
        <join-column name="jjo_grp_id" referenced-column-name="jjo_grp_id" />
    </one-to-one>


<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
