<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.RentRateDtl" access="FIELD">
     <table name="trent_rate_dtl" />

     <attributes>
        <id name="rentRateDtlId">
            <column name="rent_rate_dtl_id" />
            <generated-value strategy="TABLE" generator="TRENT_RATE_DTL_SEQ" />
            <table-generator name="TRENT_RATE_DTL_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="trent_rate_dtl_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="otherDesc">
            <column name="other_desc" />
        </basic>

        <basic name="rentRateAmt">
            <column name="rent_rate_amt" />
        </basic>

        <basic name="day">
            <column name="day" />
        </basic>

        <basic name="month">
            <column name="month" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="rentRateType" fetch="LAZY">
        <join-column name="rent_rate_type_id" referenced-column-name="rent_rate_type_id" />
    </many-to-one>

    <many-to-one name="dstrntCaseDtl" fetch="LAZY">
        <join-column name="dstrnt_case_dtl_id" referenced-column-name="dstrnt_case_dtl_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
