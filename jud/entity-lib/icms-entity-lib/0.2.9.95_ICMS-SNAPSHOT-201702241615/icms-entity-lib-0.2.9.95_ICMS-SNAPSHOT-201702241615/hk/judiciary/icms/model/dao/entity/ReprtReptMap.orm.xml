<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.ReprtReptMap" access="FIELD">
     <table name="creprt_rept_map" />

     <attributes>
        <id name="reprtReptMapId">
            <column name="reprt_rept_map_id" />
            <generated-value strategy="TABLE" generator="CREPRT_REPT_MAP_SEQ" />
            <table-generator name="CREPRT_REPT_MAP_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="creprt_rept_map_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="refunReptFlag">
            <column name="refun_rept_flag" />
        </basic>

        <basic name="seqNo">
            <column name="seq_no" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="reptFmtType" fetch="LAZY">
        <join-column name="rept_fmt_type_id" referenced-column-name="rept_fmt_type_id" />
    </many-to-one>

    <many-to-one name="prntReptFmtType" fetch="LAZY">
        <join-column name="prnt_rept_fmt_type_id" referenced-column-name="rept_fmt_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
