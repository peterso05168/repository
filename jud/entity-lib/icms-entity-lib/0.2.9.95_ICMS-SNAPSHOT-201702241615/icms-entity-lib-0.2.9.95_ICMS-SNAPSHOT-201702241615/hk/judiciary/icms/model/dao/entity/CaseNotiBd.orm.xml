<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.CaseNotiBd" access="FIELD">
     <table name="tcase_noti_bd" />

     <attributes>
        <id name="caseNotiBdId">
            <column name="case_noti_bd_id" />
            <generated-value strategy="TABLE" generator="TCASE_NOTI_BD_SEQ" />
            <table-generator name="TCASE_NOTI_BD_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tcase_noti_bd_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="notiBdEffvDate">
            <column name="noti_bd_effv_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="notiBdExyDate">
            <column name="noti_bd_exy_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="vcase" fetch="LAZY">
        <join-column name="case_id" referenced-column-name="case_id" />
    </many-to-one>

    <many-to-one name="notiBd" fetch="LAZY">
        <join-column name="noti_bd_id" referenced-column-name="noti_bd_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
