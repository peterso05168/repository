<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.HkDistToBlfDist" access="FIELD">
     <table name="chk_dist_to_blf_dist" />

     <attributes>
        <id name="hkDistToBlfDistId">
            <column name="hk_dist_to_blf_dist_id" />
            <generated-value strategy="TABLE" generator="CHK_DIST_TO_BLF_DIST_SEQ" />
            <table-generator name="CHK_DIST_TO_BLF_DIST_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="chk_dist_to_blf_dist_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="blfDist" fetch="LAZY">
        <join-column name="blf_dist_id" referenced-column-name="blf_dist_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->
    <one-to-one name="hkDist" fetch="LAZY">
        <join-column name="hk_dist_id" referenced-column-name="hk_dist_id" />
    </one-to-one>


<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
