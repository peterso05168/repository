<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.JjoRosterCtatNo" access="FIELD">
     <table name="cjjo_roster_ctat_no" />

     <attributes>
        <id name="jjoRosterCtatNoId">
            <column name="jjo_roster_ctat_no_id" />
            <generated-value strategy="TABLE" generator="CJJO_ROSTER_CTAT_NO_SEQ" />
            <table-generator name="CJJO_ROSTER_CTAT_NO_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="cjjo_roster_ctat_no_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="seq">
            <column name="seq" />
        </basic>

        <basic name="ctatNo">
            <column name="ctat_no" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="jjoRosterType" fetch="LAZY">
        <join-column name="jjo_roster_type_id" referenced-column-name="jjo_roster_type_id" />
    </many-to-one>

    <many-to-one name="hkRgn" fetch="LAZY">
        <join-column name="hk_rgn_id" referenced-column-name="hk_rgn_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
