<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.ElecSign" access="FIELD">
     <table name="telec_sign" />

     <attributes>
        <id name="elecSignId">
            <column name="elec_sign_id" />
            <generated-value strategy="TABLE" generator="TELEC_SIGN_SEQ" />
            <table-generator name="TELEC_SIGN_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="telec_sign_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="effvStartDt">
            <column name="effv_start_dt" />
        </basic>

        <basic name="effvEndDt">
            <column name="effv_end_dt" />
        </basic>

        <basic name="path">
            <column name="path" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="intlUserAc" fetch="LAZY">
        <join-column name="intl_user_ac_id" referenced-column-name="intl_user_ac_id" />
    </many-to-one>

    <many-to-one name="elecSignType" fetch="LAZY">
        <join-column name="elec_sign_type_id" referenced-column-name="elec_sign_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->
    <one-to-many name="affixDtl"  mapped-by="elecSign" fetch="LAZY" />

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
