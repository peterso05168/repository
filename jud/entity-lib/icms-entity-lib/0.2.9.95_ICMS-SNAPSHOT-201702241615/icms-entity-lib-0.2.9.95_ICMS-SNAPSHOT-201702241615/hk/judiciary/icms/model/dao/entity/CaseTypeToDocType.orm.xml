<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.CaseTypeToDocType" access="FIELD">
     <table name="ccase_type_to_doc_type" />

     <attributes>
        <id name="caseTypeToDocTypeId">
            <column name="case_type_to_doc_type_id" />
            <generated-value strategy="TABLE" generator="CCASE_TYPE_TO_DOC_TYPE_SEQ" />
            <table-generator name="CCASE_TYPE_TO_DOC_TYPE_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="ccase_type_to_doc_type_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="caseOriginatingFlag">
            <column name="case_originating_flag" />
        </basic>

        <basic name="dfltDispFlag">
            <column name="dflt_disp_flag" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="caseType" fetch="LAZY">
        <join-column name="case_type_id" referenced-column-name="case_type_id" />
    </many-to-one>

    <many-to-one name="docType" fetch="LAZY">
        <join-column name="doc_type_id" referenced-column-name="doc_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
