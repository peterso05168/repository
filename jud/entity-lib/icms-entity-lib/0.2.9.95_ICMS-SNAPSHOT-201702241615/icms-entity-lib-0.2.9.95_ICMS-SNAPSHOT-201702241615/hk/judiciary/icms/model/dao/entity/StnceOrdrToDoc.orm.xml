<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.StnceOrdrToDoc" access="FIELD">
     <table name="tstnce_ordr_to_doc" />

     <attributes>
        <id name="stnceOrdrToDocId">
            <column name="stnce_ordr_to_doc_id" />
            <generated-value strategy="TABLE" generator="TSTNCE_ORDR_TO_DOC_SEQ" />
            <table-generator name="TSTNCE_ORDR_TO_DOC_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tstnce_ordr_to_doc_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="servSpecialInstr">
            <column name="serv_special_instr" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="stnceOrdr" fetch="LAZY">
        <join-column name="stnce_ordr_id" referenced-column-name="stnce_ordr_id" />
    </many-to-one>

    <many-to-one name="docIdOfTheStnceOrdr" fetch="LAZY">
        <join-column name="doc_id_of_the_stnce_ordr" referenced-column-name="doc_id" />
    </many-to-one>

    <many-to-one name="servMode" fetch="LAZY">
        <join-column name="serv_mode_id" referenced-column-name="serv_mode_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
