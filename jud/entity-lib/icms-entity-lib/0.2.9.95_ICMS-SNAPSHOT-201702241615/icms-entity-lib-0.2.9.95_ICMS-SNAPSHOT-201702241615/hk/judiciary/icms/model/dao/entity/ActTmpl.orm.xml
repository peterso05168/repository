<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.ActTmpl" access="FIELD">
     <table name="cact_tmpl" />

     <attributes>
        <id name="actTmplId">
            <column name="act_tmpl_id" />
            <generated-value strategy="TABLE" generator="CACT_TMPL_SEQ" />
            <table-generator name="CACT_TMPL_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="cact_tmpl_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="actTmplCd">
            <column name="act_tmpl_cd" />
        </basic>

        <basic name="actTmplDesc">
            <column name="act_tmpl_desc" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="expectOutpDocType" fetch="LAZY">
        <join-column name="expect_outp_doc_type_id" referenced-column-name="expect_outp_doc_type_id" />
    </many-to-one>

    <many-to-one name="actTmplGrp" fetch="LAZY">
        <join-column name="act_tmpl_grp_id" referenced-column-name="act_tmpl_grp_id" />
    </many-to-one>

    <many-to-one name="docTmpl" fetch="LAZY">
        <join-column name="doc_tmpl_id" referenced-column-name="doc_tmpl_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
