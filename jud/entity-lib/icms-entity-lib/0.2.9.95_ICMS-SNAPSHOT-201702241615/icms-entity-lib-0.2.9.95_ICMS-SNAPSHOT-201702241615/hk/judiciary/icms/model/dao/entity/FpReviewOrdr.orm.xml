<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.FpReviewOrdr" access="FIELD">
     <table name="tfp_review_ordr" />

     <attributes>
        <id name="fpReviewOrdrId">
            <column name="fp_review_ordr_id" />
            <generated-value strategy="TABLE" generator="TFP_REVIEW_ORDR_SEQ" />
            <table-generator name="TFP_REVIEW_ORDR_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tfp_review_ordr_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="prosObjectionFlag">
            <column name="pros_objection_flag" />
        </basic>

        <basic name="refuseAddlCost">
            <column name="refuse_addl_cost" />
        </basic>

        <basic name="refuseDueDateOpt">
            <column name="refuse_due_date_opt" />
        </basic>

        <basic name="refuseDueDateOptDesc">
            <column name="refuse_due_date_opt_desc" />
        </basic>

        <basic name="refuseDueDateDay">
            <column name="refuse_due_date_day" />
        </basic>

        <basic name="refuseDueDate">
            <column name="refuse_due_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="addlPnltyAndCost">
            <column name="addl_pnlty_and_cost" />
        </basic>

        <basic name="hrnDate">
            <column name="hrn_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="refuseDueDateMonth">
            <column name="refuse_due_date_month" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="stnceOrdr" fetch="LAZY">
        <join-column name="stnce_ordr_id" referenced-column-name="stnce_ordr_id" />
    </many-to-one>

    <many-to-one name="hrnSchd" fetch="LAZY">
        <join-column name="hrn_schd_id" referenced-column-name="hrn_schd_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
