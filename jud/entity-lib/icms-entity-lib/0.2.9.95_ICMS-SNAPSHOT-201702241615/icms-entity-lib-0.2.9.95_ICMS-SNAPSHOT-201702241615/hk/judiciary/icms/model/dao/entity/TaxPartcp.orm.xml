<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.TaxPartcp" access="FIELD">
     <table name="ttax_partcp" />

     <attributes>
        <id name="taxPartcpId">
            <column name="tax_partcp_id" />
            <generated-value strategy="TABLE" generator="TTAX_PARTCP_SEQ" />
            <table-generator name="TTAX_PARTCP_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="ttax_partcp_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="billOfCost" fetch="LAZY">
        <join-column name="bill_of_cost_id" referenced-column-name="tax_bill_of_cost_id" />
    </many-to-one>

    <many-to-one name="partcpRole" fetch="LAZY">
        <join-column name="partcp_role_id" referenced-column-name="partcp_role_id" />
    </many-to-one>

    <many-to-one name="taxDtl" fetch="LAZY">
        <join-column name="tax_dtl_id" referenced-column-name="tax_dtl_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
