<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.RsltFolupAct" access="FIELD">
     <table name="trslt_folup_act" />

     <attributes>
        <id name="rsltFolupActId">
            <column name="rslt_folup_act_id" />
            <generated-value strategy="TABLE" generator="TRSLT_FOLUP_ACT_SEQ" />
            <table-generator name="TRSLT_FOLUP_ACT_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="trslt_folup_act_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="rsltFolupSrc">
            <column name="rslt_folup_src" />
        </basic>

        <basic name="folupActDate">
            <column name="folup_act_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="folupActDeadlineDate">
            <column name="folup_act_deadline_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="respOfficer">
            <column name="resp_officer" />
        </basic>

        <basic name="folupActStatus">
            <column name="folup_act_status" />
        </basic>

        <basic name="folupActCmplDate">
            <column name="folup_act_cmpl_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="rsltFolupFr" fetch="LAZY">
        <join-column name="rslt_folup_fr_id" referenced-column-name="hrn_rslt_id" />
    </many-to-one>

    <many-to-one name="rsltFolupActType" fetch="LAZY">
        <join-column name="rslt_folup_act_type_id" referenced-column-name="rslt_folup_act_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
