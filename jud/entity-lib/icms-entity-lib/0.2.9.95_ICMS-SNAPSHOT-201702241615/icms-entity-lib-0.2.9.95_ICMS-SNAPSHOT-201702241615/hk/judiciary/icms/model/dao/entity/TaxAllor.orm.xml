<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.TaxAllor" access="FIELD">
     <table name="ttax_allor" />

     <attributes>
        <id name="taxAllorId">
            <column name="tax_allor_id" />
            <generated-value strategy="TABLE" generator="TTAX_ALLOR_SEQ" />
            <table-generator name="TTAX_ALLOR_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="ttax_allor_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="taxAllorProfitCost">
            <column name="tax_allor_profit_cost" />
        </basic>

        <basic name="taxAllorDisbmt">
            <column name="tax_allor_disbmt" />
        </basic>

        <basic name="taxFree">
            <column name="tax_free" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="taxBillOfCostItem" fetch="LAZY">
        <join-column name="tax_bill_of_cost_item_id" referenced-column-name="tax_bill_of_cost_item_id" />
    </many-to-one>

    <many-to-one name="costType" fetch="LAZY">
        <join-column name="cost_type_id" referenced-column-name="cost_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
