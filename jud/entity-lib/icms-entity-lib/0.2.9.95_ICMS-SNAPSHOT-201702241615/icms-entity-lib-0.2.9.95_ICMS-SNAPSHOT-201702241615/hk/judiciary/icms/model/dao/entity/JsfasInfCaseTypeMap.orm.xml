<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.JsfasInfCaseTypeMap" access="FIELD">
     <table name="cjsfas_inf_case_type_map" />

     <attributes>
        <id name="jsfasInfCaseTypeMapId">
            <column name="jsfas_inf_case_type_map_id" />
            <generated-value strategy="TABLE" generator="CJSFAS_INF_CASE_TYPE_MAP_SEQ" />
            <table-generator name="CJSFAS_INF_CASE_TYPE_MAP_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="cjsfas_inf_case_type_map_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="frCourtSys">
            <column name="fr_court_sys" />
        </basic>

        <basic name="frCaseType">
            <column name="fr_case_type" />
        </basic>

        <basic name="toCourtSys">
            <column name="to_court_sys" />
        </basic>

        <basic name="toCaseType">
            <column name="to_case_type" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="pymtCd" fetch="LAZY">
        <join-column name="pymt_cd_id" referenced-column-name="pymt_cd_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
