<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.FpDailySumy" access="FIELD">
     <table name="tfp_daily_sumy" />

     <attributes>
        <id name="fpDailySumyId">
            <column name="fp_daily_sumy_id" />
            <generated-value strategy="TABLE" generator="TFP_DAILY_SUMY_SEQ" />
            <table-generator name="TFP_DAILY_SUMY_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tfp_daily_sumy_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="totalCashAmt">
            <column name="total_cash_amt" />
        </basic>

        <basic name="totalChqAmt">
            <column name="total_chq_amt" />
        </basic>

        <basic name="totalEpsAmt">
            <column name="total_eps_amt" />
        </basic>

        <basic name="fpDailySumyClsgDate">
            <column name="fp_daily_sumy_clsg_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="totalCashCnt">
            <column name="total_cash_cnt" />
        </basic>

        <basic name="totalChqCnt">
            <column name="total_chq_cnt" />
        </basic>

        <basic name="totalEpsCnt">
            <column name="total_eps_cnt" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="tktDemandType" fetch="LAZY">
        <join-column name="tkt_demand_type_id" referenced-column-name="tkt_demand_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->
    <one-to-many name="fpTxnSumy"  mapped-by="fpDailySumy" fetch="LAZY" />

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
