            <column name="doc_gen_reqs_rslt_path_id" />
            <generated-value strategy="TABLE" generator="TDOC_GEN_REQS_RSLT_PATH_SEQ" />
            <table-generator name="TDOC_GEN_REQS_RSLT_PATH_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tdoc_gen_reqs_rslt_path_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="rsltFilePath">
            <column name="rslt_file_path" />
        </basic>

        <basic name="genEndDt">
            <column name="gen_end_dt" />
        </basic>

        <basic name="genStartDt">
            <column name="gen_start_dt" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="docGenReqs" fetch="LAZY">
        <join-column name="doc_gen_reqs_id" referenced-column-name="doc_gen_reqs_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

