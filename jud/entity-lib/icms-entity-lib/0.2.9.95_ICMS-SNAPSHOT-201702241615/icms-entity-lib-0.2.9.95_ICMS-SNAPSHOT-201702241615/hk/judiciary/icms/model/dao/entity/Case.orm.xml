<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.Case" access="FIELD">
     <table name="tcase" />

     <attributes>
        <id name="caseId">
            <column name="case_id" />
            <generated-value strategy="TABLE" generator="TCASE_SEQ" />
            <table-generator name="TCASE_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tcase_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="caseYr">
            <column name="case_yr" />
        </basic>

        <basic name="caseRestDate">
            <column name="case_rest_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="caseExtLocDesc">
            <column name="case_ext_loc_desc" />
        </basic>

        <basic name="hpCaseFlag">
            <column name="hp_case_flag" />
        </basic>

        <basic name="sensCaseFlag">
            <column name="sens_case_flag" />
        </basic>

        <basic name="leadCaseFlag">
            <column name="lead_case_flag" />
        </basic>

        <basic name="ackServFlag">
            <column name="ack_serv_flag" />
        </basic>

        <basic name="caseSerNo">
            <column name="case_ser_no" />
        </basic>

        <basic name="recvDate">
            <column name="recv_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="initDate">
            <column name="init_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="severaSeq">
            <column name="severa_seq" />
        </basic>

        <basic name="withStmtOfClaimFlag">
            <column name="with_stmt_of_claim_flag" />
        </basic>

        <basic name="withCnterClaimFlag">
            <column name="with_cnter_claim_flag" />
        </basic>

        <basic name="pymtReptNo">
            <column name="pymt_rept_no" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="caseType" fetch="LAZY">
        <join-column name="case_type_id" referenced-column-name="case_type_id" />
    </many-to-one>

    <many-to-one name="compsCourt" fetch="LAZY">
        <join-column name="comps_court_id" referenced-column-name="comps_court_id" />
    </many-to-one>

    <many-to-one name="notiBd" fetch="LAZY">
        <join-column name="noti_bd_id" referenced-column-name="noti_bd_id" />
    </many-to-one>

    <many-to-one name="courtLvlType" fetch="LAZY">
        <join-column name="court_lvl_type_id" referenced-column-name="court_lvl_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->
    <one-to-many name="dailyCauseList"  mapped-by="vcase" fetch="LAZY" />
    <one-to-many name="dstrntEpAppList"  mapped-by="vcase" fetch="LAZY" />
