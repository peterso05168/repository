<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.ProcGrpToPropStop" access="FIELD">
     <table name="cproc_grp_to_prop_stop" />

     <attributes>
        <id name="procGrpToPropStopId">
            <column name="proc_grp_to_prop_stop_id" />
            <generated-value strategy="TABLE" generator="CPROC_GRP_TO_PROP_STOP_SEQ" />
            <table-generator name="CPROC_GRP_TO_PROP_STOP_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="cproc_grp_to_prop_stop_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="jjoFlag">
            <column name="jjo_flag" />
        </basic>

        <basic name="dedicateOfficerFlag">
            <column name="dedicate_officer_flag" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="procOfficer" fetch="LAZY">
        <join-column name="proc_officer" referenced-column-name="intl_user_ac_id" />
    </many-to-one>

    <many-to-one name="procGrp" fetch="LAZY">
        <join-column name="proc_grp_id" referenced-column-name="proc_grp_id" />
    </many-to-one>

    <many-to-one name="docTypePropStop" fetch="LAZY">
        <join-column name="doc_type_prop_stop_id" referenced-column-name="doc_type_prop_stop_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
