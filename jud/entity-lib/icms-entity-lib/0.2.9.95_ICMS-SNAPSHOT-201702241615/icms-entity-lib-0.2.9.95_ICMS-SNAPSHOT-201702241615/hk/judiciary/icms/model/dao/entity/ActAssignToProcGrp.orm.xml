    <many-to-one name="officerBeingAssign" fetch="LAZY">
        <join-column name="officer_being_assign" referenced-column-name="intl_user_ac_id" />
    </many-to-one>

    <many-to-one name="docActAssign" fetch="LAZY">
        <join-column name="doc_act_assign_id" referenced-column-name="doc_act_assign_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

