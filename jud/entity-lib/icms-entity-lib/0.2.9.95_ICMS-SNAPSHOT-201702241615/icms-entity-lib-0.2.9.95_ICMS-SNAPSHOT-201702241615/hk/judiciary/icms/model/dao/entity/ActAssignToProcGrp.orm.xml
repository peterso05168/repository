<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.ActAssignToProcGrp" access="FIELD">
     <table name="tact_assign_to_proc_grp" />

     <attributes>
        <id name="actAssignToProcGrpId">
            <column name="act_assign_to_proc_grp_id" />
            <generated-value strategy="TABLE" generator="TACT_ASSIGN_TO_PROC_GRP_SEQ" />
            <table-generator name="TACT_ASSIGN_TO_PROC_GRP_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tact_assign_to_proc_grp_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="procGrp" fetch="LAZY">
        <join-column name="proc_grp_id" referenced-column-name="proc_grp_id" />
