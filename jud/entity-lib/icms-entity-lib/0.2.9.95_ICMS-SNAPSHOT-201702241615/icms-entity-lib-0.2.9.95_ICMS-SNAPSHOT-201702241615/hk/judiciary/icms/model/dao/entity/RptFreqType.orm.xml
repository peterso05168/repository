<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.RptFreqType" access="FIELD">
     <table name="crpt_freq_type" />

     <attributes>
        <id name="rptFreqTypeId">
            <column name="rpt_freq_type_id" />
            <generated-value strategy="TABLE" generator="CRPT_FREQ_TYPE_SEQ" />
            <table-generator name="CRPT_FREQ_TYPE_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="crpt_freq_type_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="rptFreqTypeCd">
            <column name="rpt_freq_type_cd" />
        </basic>

        <basic name="rptFreqTypeDesc">
            <column name="rpt_freq_type_desc" />
        </basic>

        <basic name="rptFreqTypeDescChin">
            <column name="rpt_freq_type_desc_chin" />
        </basic>

        <basic name="rptFreqTypeDescChinSimpl">
            <column name="rpt_freq_type_desc_chin_simpl" />
        </basic>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


