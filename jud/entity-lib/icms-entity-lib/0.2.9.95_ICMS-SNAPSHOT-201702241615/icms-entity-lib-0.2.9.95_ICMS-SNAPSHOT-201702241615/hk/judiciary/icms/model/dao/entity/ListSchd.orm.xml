<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.ListSchd" access="FIELD">
     <table name="tlist_schd" />

     <attributes>
        <id name="listSchdId">
            <column name="list_schd_id" />
            <generated-value strategy="TABLE" generator="TLIST_SCHD_SEQ" />
            <table-generator name="TLIST_SCHD_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tlist_schd_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="listSchdDate">
            <column name="list_schd_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="listSchdSessStartTime">
            <column name="list_schd_sess_start_time" />
        </basic>

        <basic name="forceBookInd">
            <column name="force_book_ind" />
        </basic>

        <basic name="status">
            <column name="status" />
        </basic>

        <basic name="blkInd">
            <column name="blk_ind" />
        </basic>

        <basic name="jjoOccupInd">
            <column name="jjo_occup_ind" />
        </basic>

        <basic name="courtRmOccupInd">
            <column name="court_rm_occup_ind" />
        </basic>

        <basic name="jjoUnavblInd">
            <column name="jjo_unavbl_ind" />
        </basic>

        <basic name="courtRmUnavblInd">
            <column name="court_rm_unavbl_ind" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="list" fetch="LAZY">
        <join-column name="list_id" referenced-column-name="list_id" />
    </many-to-one>

    <many-to-one name="vcase" fetch="LAZY">
        <join-column name="case_id" referenced-column-name="case_id" />
    </many-to-one>

    <many-to-one name="listType" fetch="LAZY">
        <join-column name="list_type_id" referenced-column-name="list_type_id" />
    </many-to-one>

    <many-to-one name="trialLang" fetch="LAZY">
        <join-column name="trial_lang_id" referenced-column-name="trial_lang_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->
    <one-to-many name="hrnSchd"  mapped-by="listSchd" fetch="LAZY" />
    <one-to-many name="hrnSchdHist"  mapped-by="listSchd" fetch="LAZY" />

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
