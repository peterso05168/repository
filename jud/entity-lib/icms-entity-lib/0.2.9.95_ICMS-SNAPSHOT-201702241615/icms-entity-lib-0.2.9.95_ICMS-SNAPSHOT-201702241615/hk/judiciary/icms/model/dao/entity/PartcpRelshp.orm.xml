<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.PartcpRelshp" access="FIELD">
     <table name="tpartcp_relshp" />

     <attributes>
        <id name="partcpRelshpId">
            <column name="partcp_relshp_id" />
            <generated-value strategy="TABLE" generator="TPARTCP_RELSHP_SEQ" />
            <table-generator name="TPARTCP_RELSHP_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tpartcp_relshp_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="relshpEffvDate">
            <column name="relshp_effv_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="relshpEndDate">
            <column name="relshp_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="ptyA" fetch="LAZY">
        <join-column name="pty_a_id" referenced-column-name="partcp_id" />
    </many-to-one>

    <many-to-one name="ptyB" fetch="LAZY">
        <join-column name="pty_b_id" referenced-column-name="partcp_id" />
    </many-to-one>

    <many-to-one name="partcpRelshpType" fetch="LAZY">
        <join-column name="partcp_relshp_type_id" referenced-column-name="partcp_relshp_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
