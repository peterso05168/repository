<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.ReqsPartcpRole" access="FIELD">
     <table name="treqs_partcp_role" />

     <attributes>
        <id name="reqsPartcpRoleId">
            <column name="reqs_partcp_role_id" />
            <generated-value strategy="TABLE" generator="TREQS_PARTCP_ROLE_SEQ" />
            <table-generator name="TREQS_PARTCP_ROLE_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="treqs_partcp_role_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="recipReqserInd">
            <column name="recip_reqser_ind" />
        </basic>

        <basic name="respondentSeqNo">
            <column name="respondent_seq_no" />
        </basic>

        <basic name="respondentSubseqNo">
            <column name="respondent_subseq_no" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="reqs" fetch="LAZY">
        <join-column name="reqs_id" referenced-column-name="reqs_id" />
    </many-to-one>

    <many-to-one name="partcpRoleType" fetch="LAZY">
        <join-column name="partcp_role_type_id" referenced-column-name="partcp_role_type_id" />
    </many-to-one>

    <many-to-one name="partcp" fetch="LAZY">
        <join-column name="partcp_id" referenced-column-name="partcp_id" />
    </many-to-one>

    <many-to-one name="origPartcpRole" fetch="LAZY">
        <join-column name="orig_partcp_role_id" referenced-column-name="partcp_role_id" />
    </many-to-one>

    <many-to-one name="ptyCat" fetch="LAZY">
        <join-column name="pty_cat_id" referenced-column-name="pty_cat_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
