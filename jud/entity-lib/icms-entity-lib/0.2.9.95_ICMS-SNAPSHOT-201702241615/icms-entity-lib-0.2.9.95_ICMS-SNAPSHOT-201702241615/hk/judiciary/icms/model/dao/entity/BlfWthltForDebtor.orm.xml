     <attributes>
        <id name="blfWthltForDebtorId">
            <column name="blf_wthlt_for_debtor_id" />
            <generated-value strategy="TABLE" generator="TBLF_WTHLT_FOR_DEBTOR_SEQ" />
            <table-generator name="TBLF_WTHLT_FOR_DEBTOR_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tblf_wthlt_for_debtor_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="mobiNo">
            <column name="mobi_no" />
        </basic>

        <basic name="telNo">
            <column name="tel_no" />
        </basic>

        <basic name="withdrawnDate">
            <column name="withdrawn_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="note">
            <column name="note" />
        </basic>

        <basic name="idNo">
            <column name="id_no" />
        </basic>

        <basic name="caseNo">
            <column name="case_no" />
        </basic>

        <basic name="wthltedDate">
            <column name="wthlted_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="arrtedDate">
            <column name="arrted_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="name">
            <column name="name" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="personIdType" fetch="LAZY">
        <join-column name="person_id_type_id" referenced-column-name="person_id_type_id" />
    </many-to-one>

    <many-to-one name="blfJdWthltStatustype" fetch="LAZY">
        <join-column name="blf_jd_wthlt_statustype_id" referenced-column-name="blf_jd_wthlt_statustype_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

