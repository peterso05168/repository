<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.BlfMealDtl" access="FIELD">
     <table name="tblf_meal_dtl" />

     <attributes>
        <id name="mealDtlId">
            <column name="meal_dtl_id" />
            <generated-value strategy="TABLE" generator="TBLF_MEAL_DTL_SEQ" />
            <table-generator name="TBLF_MEAL_DTL_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tblf_meal_dtl_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="mealStartTime">
            <column name="meal_start_time" />
        </basic>

        <basic name="mealEndTime">
            <column name="meal_end_time" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="blfAsgset" fetch="LAZY">
        <join-column name="blf_asgset_id" referenced-column-name="blf_asgset_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
