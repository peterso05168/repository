<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.VerifOfDocImageReqs" access="FIELD">
     <table name="tverif_of_doc_image_reqs" />

     <attributes>
        <id name="verifOfDocImageReqsId">
            <column name="verif_of_doc_image_reqs_id" />
            <generated-value strategy="TABLE" generator="TVERIF_OF_DOC_IMAGE_REQS_SEQ" />
            <table-generator name="TVERIF_OF_DOC_IMAGE_REQS_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tverif_of_doc_image_reqs_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="acsCd">
            <column name="acs_cd" />
        </basic>

        <basic name="deliverEmailAddr">
            <column name="deliver_email_addr" />
        </basic>

        <basic name="reqsDate">
            <column name="reqs_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="valEndDate">
            <column name="val_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="lastAcsDate">
            <column name="last_acs_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="docFolio" fetch="LAZY">
        <join-column name="doc_folio_id" referenced-column-name="doc_folio_id" />
    </many-to-one>

    <many-to-one name="acsCdDlvryChType" fetch="LAZY">
        <join-column name="acs_cd_dlvry_ch_type_id" referenced-column-name="acs_cd_dlvry_ch_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
