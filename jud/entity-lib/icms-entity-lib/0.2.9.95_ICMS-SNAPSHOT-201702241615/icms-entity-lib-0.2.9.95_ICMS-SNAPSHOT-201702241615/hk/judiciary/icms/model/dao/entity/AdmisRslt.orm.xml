<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.AdmisRslt" access="FIELD">
     <table name="tadmis_rslt" />

     <attributes>
        <id name="admisRsltId">
            <column name="admis_rslt_id" />
            <generated-value strategy="TABLE" generator="TADMIS_RSLT_SEQ" />
            <table-generator name="TADMIS_RSLT_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tadmis_rslt_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="procDate">
            <column name="proc_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="courtDir">
            <column name="court_dir" />
        </basic>

        <basic name="judgtSum">
            <column name="judgt_sum" />
        </basic>

        <basic name="noOfInstals">
            <column name="no_of_instals" />
        </basic>

        <basic name="ordrDtl">
            <column name="ordr_dtl" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="docRteHist" fetch="LAZY">
        <join-column name="doc_rte_hist_id" referenced-column-name="doc_rte_hist_id" />
    </many-to-one>

    <many-to-one name="appvStatus" fetch="LAZY">
        <join-column name="appv_status_id" referenced-column-name="admis_appv_status_id" />
    </many-to-one>

    <many-to-one name="procBy" fetch="LAZY">
        <join-column name="proc_by" referenced-column-name="intl_user_ac_id" />
    </many-to-one>

    <many-to-one name="curcyCd" fetch="LAZY">
        <join-column name="curcy_cd_id" referenced-column-name="curcy_cd_id" />
    </many-to-one>

    <many-to-one name="fixOrSumyAsesOfCost" fetch="LAZY">
        <join-column name="fix_or_sumy_ases_of_cost_id" referenced-column-name="fix_or_sumy_ases_of_cost_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->
    <one-to-many name="admisOrdrMade"  mapped-by="admisRslt" fetch="LAZY" />

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
