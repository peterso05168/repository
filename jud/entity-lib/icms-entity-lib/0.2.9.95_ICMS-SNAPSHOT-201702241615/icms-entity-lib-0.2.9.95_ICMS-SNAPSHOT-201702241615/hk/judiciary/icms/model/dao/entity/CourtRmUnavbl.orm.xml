<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.CourtRmUnavbl" access="FIELD">
     <table name="tcourt_rm_unavbl" />

     <attributes>
        <id name="courtRmUnavblId">
            <column name="court_rm_unavbl_id" />
            <generated-value strategy="TABLE" generator="TCOURT_RM_UNAVBL_SEQ" />
            <table-generator name="TCOURT_RM_UNAVBL_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tcourt_rm_unavbl_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="courtRmUnavblStatus">
            <column name="court_rm_unavbl_status" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="courtRmChmbr" fetch="LAZY">
        <join-column name="court_rm_chmbr_id" referenced-column-name="court_rm_chmbr_id" />
    </many-to-one>

    <many-to-one name="courtRmUnavblRsn" fetch="LAZY">
        <join-column name="court_rm_unavbl_rsn_id" referenced-column-name="court_rm_unavbl_rsn_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
