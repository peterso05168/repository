<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.IntmTordr" access="FIELD">
     <table name="tintm_tordr" />

     <attributes>
        <id name="intmTordrId">
            <column name="intm_tordr_id" />
            <generated-value strategy="TABLE" generator="TINTM_TORDR_SEQ" />
            <table-generator name="TINTM_TORDR_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tintm_tordr_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="ordrId">
            <column name="ordr_id" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="pymtInItem" fetch="LAZY">
        <join-column name="pymt_in_item_id" referenced-column-name="pymt_in_item_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
