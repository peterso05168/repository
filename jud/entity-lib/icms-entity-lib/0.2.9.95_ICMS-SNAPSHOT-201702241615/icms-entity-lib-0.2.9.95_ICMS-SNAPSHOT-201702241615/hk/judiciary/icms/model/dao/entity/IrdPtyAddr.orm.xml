<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.IrdPtyAddr" access="FIELD">
     <table name="tird_pty_addr" />

     <attributes>
        <id name="irdPtyAddrId">
            <column name="ird_pty_addr_id" />
            <generated-value strategy="TABLE" generator="TIRD_PTY_ADDR_SEQ" />
            <table-generator name="TIRD_PTY_ADDR_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tird_pty_addr_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="ptyNo">
            <column name="pty_no" />
        </basic>

        <basic name="addrInteNo">
            <column name="addr_inte_no" />
        </basic>

        <basic name="addr1">
            <column name="addr1" />
        </basic>

        <basic name="addr2">
            <column name="addr2" />
        </basic>

        <basic name="addr3">
            <column name="addr3" />
        </basic>

        <basic name="terr">
            <column name="terr" />
        </basic>

        <basic name="addrChin1">
            <column name="addr_chin_1" />
        </basic>

        <basic name="addrChin2">
            <column name="addr_chin_2" />
        </basic>

        <basic name="addrChin3">
            <column name="addr_chin_3" />
        </basic>

        <basic name="caseType">
            <column name="case_type" />
        </basic>

        <basic name="actNo">
            <column name="act_no" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->

<!-- Foreign key one-to-many -->

