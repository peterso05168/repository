<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.PdListCharcs" access="FIELD">
     <table name="tpd_list_charcs" />

     <attributes>
        <id name="pdListCharcsId">
            <column name="pd_list_charcs_id" />
            <generated-value strategy="TABLE" generator="TPD_LIST_CHARCS_SEQ" />
            <table-generator name="TPD_LIST_CHARCS_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tpd_list_charcs_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="dayFrAlloc">
            <column name="day_fr_alloc" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="fpAppNatType" fetch="LAZY">
        <join-column name="fp_app_nat_type_id" referenced-column-name="fp_app_nat_type_id" />
    </many-to-one>

    <many-to-one name="listCharcs" fetch="LAZY">
        <join-column name="list_charcs_id" referenced-column-name="list_charcs_id" />
    </many-to-one>

    <many-to-one name="pd" fetch="LAZY">
        <join-column name="pd_id" referenced-column-name="pd_id" />
    </many-to-one>

    <many-to-one name="caseType" fetch="LAZY">
        <join-column name="case_type_id" referenced-column-name="case_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
