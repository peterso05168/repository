                pk-column-name="seq_name" pk-column-value="tadmy_arrt_info_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="dayOfFood">
            <column name="day_of_food" />
        </basic>

        <basic name="foOilInBunker">
            <column name="fo_oil_in_bunker" />
        </basic>

        <basic name="engineRunFlag">
            <column name="engine_run_flag" />
        </basic>

        <basic name="dayOfDrinkWater">
            <column name="day_of_drink_water" />
        </basic>

        <basic name="noteOfShipPaper">
            <column name="note_of_ship_paper" />
        </basic>

        <basic name="noteOfFreight">
            <column name="note_of_freight" />
        </basic>

        <basic name="vsslGrossTonn">
            <column name="vssl_gross_tonn" />
        </basic>

        <basic name="vsslNetTonn">
            <column name="vssl_net_tonn" />
        </basic>

        <basic name="vsslLen">
            <column name="vssl_len" />
        </basic>

        <basic name="vsslBreadthMoulded">
            <column name="vssl_breadth_moulded" />
        </basic>

        <basic name="crewLastPayDate">
            <column name="crew_last_pay_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="crewInGoodHealth">
            <column name="crew_in_good_health" />
        </basic>

        <basic name="keroseneOnVssl">
            <column name="kerosene_on_vssl" />
        </basic>

        <basic name="doOilInBunker">
            <column name="do_oil_in_bunker" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="blfTaskRslt" fetch="LAZY">
        <join-column name="blf_task_rslt_id" referenced-column-name="blf_task_rslt_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

