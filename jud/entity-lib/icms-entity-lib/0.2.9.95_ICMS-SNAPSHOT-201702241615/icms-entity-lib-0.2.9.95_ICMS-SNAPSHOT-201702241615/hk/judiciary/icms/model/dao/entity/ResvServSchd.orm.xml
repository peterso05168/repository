<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.ResvServSchd" access="FIELD">
     <table name="tresv_serv_schd" />

     <attributes>
        <id name="resvServSchdId">
            <column name="resv_serv_schd_id" />
            <generated-value strategy="TABLE" generator="TRESV_SERV_SCHD_SEQ" />
            <table-generator name="TRESV_SERV_SCHD_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tresv_serv_schd_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="schdDate">
            <column name="schd_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="status">
            <column name="status" />
        </basic>

        <basic name="surname">
            <column name="surname" />
        </basic>

        <basic name="givenName">
            <column name="given_name" />
        </basic>

        <basic name="phoneNo">
            <column name="phone_no" />
        </basic>

        <basic name="sessStartTime">
            <column name="sess_start_time" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="resvServ" fetch="LAZY">
        <join-column name="resv_serv_id" referenced-column-name="resv_serv_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
