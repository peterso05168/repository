<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.ElecFileLoc" access="FIELD">
     <table name="telec_file_loc" />

     <attributes>
        <id name="elecFileLocId">
            <column name="elec_file_loc_id" />
            <generated-value strategy="TABLE" generator="TELEC_FILE_LOC_SEQ" />
            <table-generator name="TELEC_FILE_LOC_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="telec_file_loc_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="repoFileId">
            <column name="repo_file_id" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="doc" fetch="LAZY">
        <join-column name="doc_id" referenced-column-name="doc_id" />
    </many-to-one>

    <many-to-one name="docCopyType" fetch="LAZY">
        <join-column name="doc_copy_type_id" referenced-column-name="doc_copy_type_id" />
    </many-to-one>

    <many-to-one name="fileRepo" fetch="LAZY">
        <join-column name="file_repo_id" referenced-column-name="file_repo_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
