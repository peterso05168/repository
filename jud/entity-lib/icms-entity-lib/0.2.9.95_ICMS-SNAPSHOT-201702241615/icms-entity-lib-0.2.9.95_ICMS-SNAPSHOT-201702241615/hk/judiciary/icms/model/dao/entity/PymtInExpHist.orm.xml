<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.PymtInExpHist" access="FIELD">
     <table name="tpymt_in_exp_hist" />

     <attributes>
        <id name="pymtInExpHistId">
            <column name="pymt_in_exp_hist_id" />
            <generated-value strategy="TABLE" generator="TPYMT_IN_EXP_HIST_SEQ" />
            <table-generator name="TPYMT_IN_EXP_HIST_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tpymt_in_exp_hist_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="expDtFr">
            <column name="exp_dt_fr" />
        </basic>

        <basic name="expDtTo">
            <column name="exp_dt_to" />
        </basic>

        <basic name="procBy">
            <column name="proc_by" />
        </basic>

        <basic name="procDt">
            <column name="proc_dt" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="pymtWs" fetch="LAZY">
        <join-column name="pymt_ws_id" referenced-column-name="pymt_ws_id" />
    </many-to-one>

    <many-to-one name="sysType" fetch="LAZY">
        <join-column name="sys_type_id" referenced-column-name="sys_type_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

