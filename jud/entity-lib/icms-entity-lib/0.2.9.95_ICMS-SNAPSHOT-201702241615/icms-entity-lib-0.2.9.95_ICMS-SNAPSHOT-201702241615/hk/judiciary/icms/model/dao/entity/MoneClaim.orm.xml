<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.MoneClaim" access="FIELD">
     <table name="tmone_claim" />

     <attributes>
        <id name="moneClaimId">
            <column name="mone_claim_id" />
            <generated-value strategy="TABLE" generator="TMONE_CLAIM_SEQ" />
            <table-generator name="TMONE_CLAIM_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tmone_claim_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="moneSeqNo">
            <column name="mone_seq_no" />
        </basic>

        <basic name="claimRsltTotalMoneAmt">
            <column name="claim_rslt_total_mone_amt" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="curcyCd" fetch="LAZY">
        <join-column name="curcy_cd_id" referenced-column-name="curcy_cd_id" />
    </many-to-one>

    <many-to-one name="vcase" fetch="LAZY">
        <join-column name="case_id" referenced-column-name="case_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
