<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.PymtOutItem" access="FIELD">
     <table name="tpymt_out_item" />

     <attributes>
        <id name="pymtOutItemId">
            <column name="pymt_out_item_id" />
            <generated-value strategy="TABLE" generator="TPYMT_OUT_ITEM_SEQ" />
            <table-generator name="TPYMT_OUT_ITEM_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tpymt_out_item_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="ordrId">
            <column name="ordr_id" />
        </basic>

        <basic name="ordrTypeId">
            <column name="ordr_type_id" />
        </basic>

        <basic name="pymtOutItemIssueDate">
            <column name="pymt_out_item_issue_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="txnFundAcTypeId">
            <column name="txn_fund_ac_type_id" />
        </basic>

        <basic name="docId">
            <column name="doc_id" />
        </basic>

        <basic name="pymtAdvNo">
            <column name="pymt_adv_no" />
        </basic>

        <basic name="pymtOutItemAmt">
            <column name="pymt_out_item_amt" />
        </basic>

        <basic name="stnceOrdrId">
            <column name="stnce_ordr_id" />
        </basic>

        <basic name="ptyGivenName">
            <column name="pty_given_name" />
        </basic>

        <basic name="ptyOrgName">
            <column name="pty_org_name" />
        </basic>

        <basic name="ptySurname">
            <column name="pty_surname" />
        </basic>

        <basic name="ptyDocNo">
            <column name="pty_doc_no" />
        </basic>

        <basic name="pymtOutItemDueDate">
            <column name="pymt_out_item_due_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="submitToHighCourtDate">
            <column name="submit_to_high_court_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="pymtStatusType" fetch="LAZY">
        <join-column name="pymt_status_type_id" referenced-column-name="pymt_status_type_id" />
    </many-to-one>

    <many-to-one name="pymtSubCd" fetch="LAZY">
        <join-column name="pymt_sub_cd_id" referenced-column-name="pymt_sub_cd_id" />
    </many-to-one>

    <many-to-one name="curcyCd" fetch="LAZY">
        <join-column name="curcy_cd_id" referenced-column-name="curcy_cd_id" />
    </many-to-one>

    <many-to-one name="intmCase" fetch="LAZY">
        <join-column name="intm_case_id" referenced-column-name="intm_case_id" />
    </many-to-one>

    <many-to-one name="ptyRole" fetch="LAZY">
        <join-column name="pty_role_id" referenced-column-name="pty_role_id" />
    </many-to-one>

    <many-to-one name="recvCompsCourt" fetch="LAZY">
        <join-column name="recv_comps_court_id" referenced-column-name="comps_court_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->
    <one-to-many name="baRegItem"  mapped-by="pymtOutItem" fetch="LAZY" />
    <one-to-many name="pymtOutTxnItemHist"  mapped-by="pymtOutItem" fetch="LAZY" />

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
