<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.PropTaskActToActTmpl" access="FIELD">
     <table name="cprop_task_act_to_act_tmpl" />

     <attributes>
        <id name="propTaskActToActTmplId">
            <column name="prop_task_act_to_act_tmpl_id" />
            <generated-value strategy="TABLE" generator="CPROP_TASK_ACT_TO_ACT_TMPL_SEQ" />
            <table-generator name="CPROP_TASK_ACT_TO_ACT_TMPL_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="cprop_task_act_to_act_tmpl_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="effvStartDate">
            <column name="effv_start_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="effvEndDate">
            <column name="effv_end_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="activeFlag">
            <column name="active_flag" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
    <many-to-one name="propTaskAct" fetch="LAZY">
        <join-column name="prop_task_act_id" referenced-column-name="prop_task_act_id" />
    </many-to-one>

    <many-to-one name="actTmpl" fetch="LAZY">
        <join-column name="act_tmpl_id" referenced-column-name="act_tmpl_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
