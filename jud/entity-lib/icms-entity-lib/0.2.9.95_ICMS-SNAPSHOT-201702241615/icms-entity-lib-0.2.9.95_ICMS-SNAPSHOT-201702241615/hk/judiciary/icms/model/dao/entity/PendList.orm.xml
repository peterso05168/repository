<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
     <entity class="hk.judiciary.icms.model.dao.entity.PendList" access="FIELD">
     <table name="tpend_list" />

     <attributes>
        <id name="pendListId">
            <column name="pend_list_id" />
            <generated-value strategy="TABLE" generator="TPEND_LIST_SEQ" />
            <table-generator name="TPEND_LIST_SEQ" table="TFMK_SEQ_NO" 
                pk-column-name="seq_name" pk-column-value="tpend_list_id" 
                value-column-name="SEQ_CNT" allocation-size="1" />
        </id>

        <basic name="pendListWarnedDate">
            <column name="pend_list_warned_date" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="monthYrOfPendList">
            <column name="month_yr_of_pend_list" />
        </basic>

        <basic name="pendListGenDt">
            <column name="pend_list_gen_dt" />
            <temporal>TIMESTAMP</temporal>
        </basic>

        <basic name="pendListOtherRelevInfo">
            <column name="pend_list_other_relev_info" />
        </basic>


        <version name="version">
            <column name="version_dt" />
        </version>


<!-- Foreign key many-to-one -->
 <!--
    <many-to-one name="runList" fetch="LAZY">
        <join-column name="run_list_id" referenced-column-name="run_list_id" />
    </many-to-one>

 -->

    <many-to-one name="vcase" fetch="LAZY">
        <join-column name="case_id" referenced-column-name="case_id" />
    </many-to-one>

    <many-to-one name="pendListGenReqsBy" fetch="LAZY">
        <join-column name="pend_list_gen_reqs_by" referenced-column-name="intl_user_ac_id" />
    </many-to-one>

    <many-to-one name="pendListReviewBy" fetch="LAZY">
        <join-column name="pend_list_review_by" referenced-column-name="intl_user_ac_id" />
    </many-to-one>

    <many-to-one name="pendListAppvBy" fetch="LAZY">
        <join-column name="pend_list_appv_by" referenced-column-name="intl_user_ac_id" />
    </many-to-one>


<!-- Foreign key one-to-many -->

<!-- Foreign key one-to-one -->

<!-- Foreign key many-to-many -->
     </attributes>
     </entity>
</entity-mappings>
